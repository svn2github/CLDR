<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="ky"/>
	</identity>
	<collations>
		<collation type="standard">
			<settings reorder="Cyrl"/>
			<!-- Standard Cyrillic sort order,
			  see http://en.wikipedia.org/wiki/Kyrgyz_alphabets#Correspondence_chart
			  Tilek Mamutov (Google) confirms that й should sort as a base letter,
			  but ё should sort secondary-after е. -->
			<!-- The root collation already sorts й/Й as a base letter. -->
		</collation>
	</collations>
</ldml>
