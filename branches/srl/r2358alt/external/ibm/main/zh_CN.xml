      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>H'&#x65f6;'m'&#x5206;'s'&#x79d2;'</full>
          <medium>H:m:s</medium>
          <short>H:m</short>
        </time>

        <date>
          <full>yyyy'&#x5e74;'MMMMd'&#x65e5;' EEEE</full>
          <long>yyyy'&#x5e74;'MMMMd'&#x65e5;'</long>
          <short>yy-M-d</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>.</decimal>
      <group>,</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="CNY">
      <symbol>&#xffe5;</symbol>
      <pattern>&#x00A4;#,###,##0;-&#x00A4;#,###,##0</pattern>
      <decimal>.</decimal>
      <group>,</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>&#x5426;</no>
      <yes>&#x662f;</yes>
      <shortNo>&#x5426;</shortNo>
      <shortYes>&#x662f;</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

