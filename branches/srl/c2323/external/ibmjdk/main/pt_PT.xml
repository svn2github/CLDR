					</monthContext>
				</months>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##0%</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0.00 ¤;-#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
		<currencies>
			<default type="EUR"/>
			<currency type="EUR">
				<decimal>$</decimal>
			</currency>
		</currencies>
	</numbers>
</ldml>
