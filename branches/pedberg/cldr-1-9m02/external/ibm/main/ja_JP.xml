<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>.</decimal>
      <group>,</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="JPY">
      <symbol>&#x00A5;</symbol>
      <pattern>&#xa5;#,###,###;-&#xa5;#,###,###</pattern>
      <decimal></decimal>
      <group>,</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>&#x3044;&#x3044;&#x3048;</no>
      <yes>&#x306F;&#x3044;</yes>
      <shortNo></shortNo>
      <shortYes></shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
