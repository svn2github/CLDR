<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="pt"/>
    <country id="PT"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">Janeiro</month>
        <month id="2">Fevereiro</month>
        <month id="3">Mar&#x00e7;o</month>
        <month id="4">Abril</month>
        <month id="5">Maio</month>
        <month id="6">Junho</month>
        <month id="7">Julho</month>
        <month id="8">Agosto</month>
        <month id="9">Setembro</month>
        <month id="10">Outubro</month>
        <month id="11">Novembro</month>
        <month id="12">Dezembro</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">Jan</month>
        <month id="2">Fev</month>
        <month id="3">Mar</month>
        <month id="4">Abr</month>
        <month id="5">Mai</month>
        <month id="6">Jun</month>
        <month id="7">Jul</month>
        <month id="8">Ago</month>
        <month id="9">Set</month>
        <month id="10">Out</month>
        <month id="11">Nov</month>
        <month id="12">Dez</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">Domingo</day>
        <day id="2">Segunda&#x002d;feira</day>
        <day id="3">Ter&#x00e7;a&#x002d;feira</day>
        <day id="4">Quarta&#x002d;feira</day>
        <day id="5">Quinta&#x002d;feira</day>
        <day id="6">Sexta&#x002d;feira</day>
        <day id="7">S&#x00e1;bado</day>
      </dayNames>

      <dayAbbr>
        <day id="1">Dom</day>
        <day id="2">Seg</day>
        <day id="3">Ter</day>
        <day id="4">Qua</day>
        <day id="5">Qui</day>
        <day id="6">Sex</day>
        <day id="7">S&#x00E1;b</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE, d 'de' MMMM 'de' yyyy</full>
          <long>d 'de' MMMM 'de' yyyy</long>
          <short>dd/MM/yyyy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="EUR">
      <symbol>&#x20AC;</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>$</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>N&#x00c3;O</no>
      <yes>SIM</yes>
      <shortNo>N</shortNo>
      <shortYes>S</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
