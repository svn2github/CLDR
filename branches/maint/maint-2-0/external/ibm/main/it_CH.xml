<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="it"/>
    <country id="CH"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">gennaio</month>
        <month id="2">febbraio</month>
        <month id="3">marzo</month>
        <month id="4">aprile</month>
        <month id="5">maggio</month>
        <month id="6">giugno</month>
        <month id="7">luglio</month>
        <month id="8">agosto</month>
        <month id="9">settembre</month>
        <month id="10">ottobre</month>
        <month id="11">novembre</month>
        <month id="12">dicembre</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">gen</month>
        <month id="2">feb</month>
        <month id="3">mar</month>
        <month id="4">apr</month>
        <month id="5">mag</month>
        <month id="6">giu</month>
        <month id="7">lug</month>
        <month id="8">ago</month>
        <month id="9">set</month>
        <month id="10">ott</month>
        <month id="11">nov</month>
        <month id="12">dic</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">domenica</day>
        <day id="2">luned&#x00ec;</day>
        <day id="3">marted&#x00ec;</day>
        <day id="4">mercoled&#x00ec;</day>
        <day id="5">gioved&#x00ec;</day>
        <day id="6">venerd&#x00ec;</day>
        <day id="7">sabato</day>
      </dayNames>

      <dayAbbr>
        <day id="1">dom</day>
        <day id="2">lun</day>
        <day id="3">mar</day>
        <day id="4">mer</day>
        <day id="5">gio</day>
        <day id="6">ven</day>
        <day id="7">sab</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>H:m:s</full>
          <medium>H:m:s</medium>
          <short>H:m</short>
        </time>

        <date>
          <full>EEEE, dd MMMM yyyy</full>
          <long>dd MMMM yyyy</long>
          <short>dd.MM.yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>&#x0020;</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="CHF">
      <symbol>CHF</symbol>
      <pattern>&#x00A4; #,###,##0.00;&#x00A4; -#,###,##0.00</pattern>
      <decimal>.</decimal>
      <group>&#x0020;</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>No</no>
      <yes>S&#x00cc;</yes>
      <shortNo>N</shortNo>
      <shortYes>S</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
