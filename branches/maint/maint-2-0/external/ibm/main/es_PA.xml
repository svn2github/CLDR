<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="es"/>
    <country id="PA"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">enero</month>
        <month id="2">febrero</month>
        <month id="3">marzo</month>
        <month id="4">abril</month>
        <month id="5">mayo</month>
        <month id="6">junio</month>
        <month id="7">julio</month>
        <month id="8">agosto</month>
        <month id="9">septiembre</month>
        <month id="10">octubre</month>
        <month id="11">noviembre</month>
        <month id="12">diciembre</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">ene</month>
        <month id="2">feb</month>
        <month id="3">mar</month>
        <month id="4">abr</month>
        <month id="5">may</month>
        <month id="6">jun</month>
        <month id="7">jul</month>
        <month id="8">ago</month>
        <month id="9">sep</month>
        <month id="10">oct</month>
        <month id="11">nov</month>
        <month id="12">dic</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">domingo</day>
        <day id="2">lunes</day>
        <day id="3">martes</day>
        <day id="4">mi&#x00e9;rcoles</day>
        <day id="5">jueves</day>
        <day id="6">viernes</day>
        <day id="7">s&#x00e1;bado</day>
      </dayNames>

      <dayAbbr>
        <day id="1">dom</day>
        <day id="2">lun</day>
        <day id="3">mar</day>
        <day id="4">mi&#x00E9;</day>
        <day id="5">jue</day>
        <day id="6">vie</day>
        <day id="7">s&#x00E1;b</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>h:mm:ss a</full>
          <medium>h:mm:ss a</medium>
          <short>h:mm a</short>
        </time>

        <date>
          <full>EEEE d 'de' MMMM 'de' yyyy</full>
          <long>d 'de' MMMM 'de' yyyy</long>
          <short>dd/MM/yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>.</decimal>
      <group>,</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="PAB">
      <symbol>B/ ; $</symbol>
      <pattern>&#x00A4;###,##0.00;&#x00A4;-###,##0.00</pattern>
      <decimal>.</decimal>
      <group>,</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>No</no>
      <yes>S&#x00ED;</yes>
      <shortNo>n</shortNo>
      <shortYes>s</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
