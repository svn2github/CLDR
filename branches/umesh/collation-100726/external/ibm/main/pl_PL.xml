<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="pl"/>
    <country id="PL"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">styczen&#x0301;</month>
        <month id="2">luty</month>
        <month id="3">marzec</month>
        <month id="4">kwiecie&#x0144;</month>
        <month id="5">maj</month>
        <month id="6">czerwiec</month>
        <month id="7">lipiec</month>
        <month id="8">sierpie&#x0144;</month>
        <month id="9">wrzesie&#x0144;</month>
        <month id="10">pa&#x017a;dziernik</month>
        <month id="11">listopad</month>
        <month id="12">grudzie&#x0144;</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">sty</month>
        <month id="2">lut</month>
        <month id="3">mar</month>
        <month id="4">kwi</month>
        <month id="5">maj</month>
        <month id="6">cze</month>
        <month id="7">lip</month>
        <month id="8">sie</month>
        <month id="9">wrz</month>
        <month id="10">pa&#x017a;</month>
        <month id="11">lis</month>
        <month id="12">gru</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">niedziela</day>
        <day id="2">poniedzia&#x0142;ek</day>
        <day id="3">wtorek</day>
        <day id="4">&#x015b;roda</day>
        <day id="5">czwartek</day>
        <day id="6">pi&#x0105;tek</day>
        <day id="7">sobota</day>
      </dayNames>

      <dayAbbr>
        <day id="1">n</day>
        <day id="2">pn</day>
        <day id="3">wt</day>
        <day id="4">&#x015b;r</day>
        <day id="5">cz</day>
        <day id="6">pt</day>
        <day id="7">so</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>d MMMM yyyy 'r'. '('EEEE')'</full>
          <long>d MMMM yyyy 'r'.</long>
          <short>dd.MM.yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="PLN">
      <symbol>z&#x0142;</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>NIE</no>
      <yes>TAK</yes>
      <shortNo>N</shortNo>
      <shortYes>T</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
