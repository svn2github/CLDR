<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.2/alpha/ldml.dtd">
<ldml>
	<identity>
		<version number="1.2"></version>
		<generation date="2004-08-27"></generation>
		<language type="de"></language> 
		<territory type="LU"></territory> 
	</identity>
		<dates>
			<calendars>
				
				<calendar type="gregorian">
					<week>
						<minDays count="1"></minDays>
						<firstDay day="mon"></firstDay>
					</week>
				</calendar>
			</calendars>
		</dates>
		<numbers>
			<decimalFormats>
				<decimalFormatLength>
					<decimalFormat>
						<pattern>#,##0.###;-#,##0.###</pattern>
					</decimalFormat>
				</decimalFormatLength>
			</decimalFormats>
			<scientificFormats>
				<scientificFormatLength>
					<scientificFormat>
						<pattern>#E0</pattern>
					</scientificFormat>
				</scientificFormatLength>
			</scientificFormats>
			<percentFormats>
				<percentFormatLength>
					<percentFormat>
						<pattern>#,##0%</pattern>
					</percentFormat>
				</percentFormatLength>
			</percentFormats>
			<currencyFormats>
				<currencyFormatLength>
					<currencyFormat>
						<pattern>#,##0.00 造;-#,##0.00 造</pattern>
					</currencyFormat>
				</currencyFormatLength>
			</currencyFormats>
			<currencies>
				<currency type="LUF">
					<displayName>Luxemburgischer Franc</displayName>
					<symbol>F</symbol>
					<pattern>#,##0 造;-#,##0 造</pattern>
					<decimal>.</decimal>
					<group>,</group>
				</currency>
			</currencies>
		</numbers>
</ldml>
