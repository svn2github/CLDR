<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="ko"/>
    <country id="KR"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">1&#xc6d4;</month>
        <month id="2">2&#xc6d4;</month>
        <month id="3">3&#xc6d4;</month>
        <month id="4">4&#xc6d4;</month>
        <month id="5">5&#xc6d4;</month>
        <month id="6">6&#xc6d4;</month>
        <month id="7">7&#xc6d4;</month>
        <month id="8">8&#xc6d4;</month>
        <month id="9">9&#xc6d4;</month>
        <month id="10">10&#xc6d4;</month>
        <month id="11">11&#xc6d4;</month>
        <month id="12">12&#xc6d4;</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">1&#xc6d4;</month>
        <month id="2">2&#xc6d4;</month>
        <month id="3">3&#xc6d4;</month>
        <month id="4">4&#xc6d4;</month>
        <month id="5">5&#xc6d4;</month>
        <month id="6">6&#xc6d4;</month>
        <month id="7">7&#xc6d4;</month>
        <month id="8">8&#xc6d4;</month>
        <month id="9">9&#xc6d4;</month>
        <month id="10">10&#xc6d4;v</month>
        <month id="11">11&#xc6d4;</month>
        <month id="12">12&#xc6d4;</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">&#xc77c;&#xc694;&#xc77c;</day>
        <day id="2">&#xc6d4;&#xc694;&#xc77c;</day>
        <day id="3">&#xd654;&#xc694;&#xc77c;</day>
        <day id="4">&#xc218;&#xc694;&#xc77c;</day>
        <day id="5">&#xbaa9;&#xc694;&#xc77c;</day>
        <day id="6">&#xae08;&#xc694;&#xc77c;</day>
        <day id="7">&#xd1a0;&#xc694;&#xc77c;</day>
      </dayNames>

      <dayAbbr>
        <day id="1">&#xc77c;</day>
        <day id="2">&#xc6d4;</day>
        <day id="3">&#xd654;</day>
        <day id="4">&#xc218;</day>
        <day id="5">&#xbaa9;</day>
        <day id="6">&#xae08;</day>
        <day id="7">&#xd1a0;</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>a h:mm:ss</full>
          <medium>a h:mm:ss</medium>
          <short>a h:mm</short>
        </time>

        <date>
          <full>yyyy'&#xb144;' MMMM d'&#xc77c;' EEEE</full>
          <long>yyyy'&#xb144;' MMMM d'&#xc77c;'</long>
          <short>yy/MM/dd</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>.</decimal>
      <group>,</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="KRW">
      <symbol>&#x20A9; in half-width, @&#xFFE6; in full-width</symbol>
      <pattern>&#x00A4;#,###,###;&#x00A4;#,###,###</pattern>
      <decimal>.</decimal>
      <group>,</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>&#xC544;&#xB2C8;&#xC624;</no>
      <yes>&#xC608;</yes>
      <shortNo>&#xC544;&#xB2C8;&#xC624;</shortNo>
      <shortYes>&#xC608;</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>001; 002</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
