<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="en"/>
    <country id="IE"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">January</month>
        <month id="2">February</month>
        <month id="3">March</month>
        <month id="4">April</month>
        <month id="5">May</month>
        <month id="6">June</month>
        <month id="7">July</month>
        <month id="8">August</month>
        <month id="9">September</month>
        <month id="10">October</month>
        <month id="11">November</month>
        <month id="12">December</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">Jan</month>
        <month id="2">Feb</month>
        <month id="3">Mar</month>
        <month id="4">Apr</month>
        <month id="5">May</month>
        <month id="6">Jun</month>
        <month id="7">Jul</month>
        <month id="8">Aug</month>
        <month id="9">Sep</month>
        <month id="10">Oct</month>
        <month id="11">Nov</month>
        <month id="12">Dec</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">Sunday</day>
        <day id="2">Monday</day>
        <day id="3">Tuesday</day>
        <day id="4">Wednesday</day>
        <day id="5">Thursday</day>
        <day id="6">Friday</day>
        <day id="7">Saturday</day>
      </dayNames>

      <dayAbbr>
        <day id="1">Sun</day>
        <day id="2">Mon</day>
        <day id="3">Tue</day>
        <day id="4">Wed</day>
        <day id="5">Thu</day>
        <day id="6">Fri</day>
        <day id="7">Sat</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss z</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE, d MMMM, yyyy</full>
          <long>d MMMM, yyyy</long>
          <short>dd/MM/yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>.</decimal>
      <group>,</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="EUR">
      <symbol>&#x20AC;</symbol>
      <pattern>&#x00A4; #,###,##0.00;-&#x00A4; #,###,##0.00</pattern>
      <decimal>.</decimal>
      <group>,</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>No</no>
      <yes>Yes</yes>
      <shortNo>N</shortNo>
      <shortYes>Y</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>16</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
