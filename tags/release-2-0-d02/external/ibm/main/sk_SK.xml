<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="sk"/>
    <country id="SK"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">janu&#x00E1;r</month>
        <month id="2">febru&#x00E1;r</month>
        <month id="3">marec</month>
        <month id="4">apr&#x00ED;l</month>
        <month id="5">m&#x00E1;j</month>
        <month id="6">j&#x00FA;n</month>
        <month id="7">j&#x00FA;l</month>
        <month id="8">august</month>
        <month id="9">september</month>
        <month id="10">okt&#x00F3;ber</month>
        <month id="11">november</month>
        <month id="12">december</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">Jan</month>
        <month id="2">Feb</month>
        <month id="3">Mar</month>
        <month id="4">Apr</month>
        <month id="5">M&#x00E1;j</month>
        <month id="6">J&#x00FA;n</month>
        <month id="7">J&#x00FA;l</month>
        <month id="8">Aug</month>
        <month id="9">Sep</month>
        <month id="10">Okt</month>
        <month id="11">Nov</month>
        <month id="12">Dec</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">Nede&#x013E;a</day>
        <day id="2">Pondelok</day>
        <day id="3">Utorok</day>
        <day id="4">Streda</day>
        <day id="5">&#x0160;tvrtok</day>
        <day id="6">Piatok</day>
        <day id="7">Sobota</day>
      </dayNames>

      <dayAbbr>
        <day id="1">Ne</day>
        <day id="2">Po</day>
        <day id="3">Ut</day>
        <day id="4">St</day>
        <day id="5">&#x0160;t</day>
        <day id="6">Pia</day>
        <day id="7">So</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE, dd. MMMM yyyy</full>
          <long>dd. MMMM yyyy</long>
          <short>d.M.yyyy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="SKK">
      <symbol>Sk</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>Nie</no>
      <yes>Ano</yes>
      <shortNo>N</shortNo>
      <shortYes>A</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
