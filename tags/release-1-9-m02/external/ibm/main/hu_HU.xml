<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="hu"/>
    <country id="HU"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">janu&#x00e1;r</month>
        <month id="2">febru&#x00e1;r</month>
        <month id="3">m&#x00e1;rcius</month>
        <month id="4">&#x00e1;prilis</month>
        <month id="5">m&#x00e1;jus</month>
        <month id="6">j&#x00fa;nius</month>
        <month id="7">j&#x00fa;lius</month>
        <month id="8">augusztus</month>
        <month id="9">szeptember</month>
        <month id="10">okt&#x00f3;ber</month>
        <month id="11">november</month>
        <month id="12">december</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">jan&#x002e;</month>
        <month id="2">febr&#x002e;</month>
        <month id="3">m&#x00e1;rc&#x002e;</month>
        <month id="4">&#x00e1;pr&#x002e;</month>
        <month id="5">m&#x00e1;j&#x002e;</month>
        <month id="6">j&#x00fa;n&#x002e;</month>
        <month id="7">j&#x00fa;l&#x002e;</month>
        <month id="8">aug&#x002e;</month>
        <month id="9">szept&#x002e;</month>
        <month id="10">okt&#x002e;</month>
        <month id="11">nov&#x002e;</month>
        <month id="12">dec&#x002e;</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">vas&#x00e1;rnap</day>
        <day id="2">h&#x00e9;tf&#x0151;</day>
        <day id="3">kedd</day>
        <day id="4">szerda</day>
        <day id="5">cs&#x00fc;t&#x00f6;rt&#x00f6;k</day>
        <day id="6">p&#x00e9;ntek</day>
        <day id="7">szombat</day>
      </dayNames>

      <dayAbbr>
        <day id="1">v</day>
        <day id="2">h</day>
        <day id="3">k</day>
        <day id="4">sze</day>
        <day id="5">cs</day>
        <day id="6">p</day>
        <day id="7">szo</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>h:m:s a</full>
          <medium>h:m:s a</medium>
          <short>h:m a</short>
        </time>

        <date>
          <full>yyyy. MMMM dd., EEEE</full>
          <long>yyyy. MMMM dd.</long>
          <short>yy.MM.dd</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>&#x0020;</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="HUF">
      <symbol>Ft</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>&#x0020;</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>nem</no>
      <yes>igen</yes>
      <shortNo>N</shortNo>
      <shortYes>I</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
