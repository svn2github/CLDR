<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="tr"/>
    <country id="TR"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">Ocak</month>
        <month id="2">&#x015e;ubat</month>
        <month id="3">Mart</month>
        <month id="4">Nisan</month>
        <month id="5">May&#x0131;s</month>
        <month id="6">Haziran</month>
        <month id="7">Temmuz</month>
        <month id="8">A&#x011f;ustos</month>
        <month id="9">Eyl&#x00fc;l</month>
        <month id="10">Ekim</month>
        <month id="11">Kas&#x0131;m</month>
        <month id="12">Aral&#x0131;k</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">Oca</month>
        <month id="2">&#x015e;ub</month>
        <month id="3">Mar</month>
        <month id="4">Nis</month>
        <month id="5">May</month>
        <month id="6">Haz</month>
        <month id="7">Tem</month>
        <month id="8">A&#x011f;u</month>
        <month id="9">Eyl</month>
        <month id="10">Eki</month>
        <month id="11">Kas</month>
        <month id="12">Ara</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">Pazar</day>
        <day id="2">Pazartesi</day>
        <day id="3">Sal&#x0131;</day>
        <day id="4">&#x00c7;ar&#x015f;amba</day>
        <day id="5">Per&#x015f;embe</day>
        <day id="6">Cuma</day>
        <day id="7">Cumartesi</day>
      </dayNames>

      <dayAbbr>
        <day id="1">Paz</day>
        <day id="2">Pts</day>
        <day id="3">Sal</day>
        <day id="4">&#x00c7;ar</day>
        <day id="5">Per</day>
        <day id="6">Cum</day>
        <day id="7">Cts</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>dd MMMM yyyy EEEE</full>
          <long>dd MMMM yyyy</long>
          <short>dd/MM/yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="TRL">
      <symbol>TL</symbol>
      <pattern>#,###,##0.00&#x00A4;;-#,###,##0.00&#x00A4;</pattern>
      <decimal>,</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>Hay&#x0131;r</no>
      <yes>Evet</yes>
      <shortNo>H</shortNo>
      <shortYes>E</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
