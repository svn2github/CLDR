<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="nl"/>
		<territory type="BE"/>
	</identity>
	<delimiters>
		<quotationStart draft="true">‘</quotationStart>
		<quotationEnd draft="true">’</quotationEnd>
		<alternateQuotationStart draft="true">“</alternateQuotationStart>
		<alternateQuotationEnd draft="true">”</alternateQuotationEnd>
	</delimiters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<week>
					<minDays count="4" draft="true"/>
					<firstDay day="mon" draft="true"/>
				</week>
				<dateFormats>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>d-MMM-yy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>d/MM/yy</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>HH.mm' u. 'z</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern>HH:mm:ss z</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="medium">
						<timeFormat>
							<pattern>HH:mm:ss</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="short">
						<timeFormat>
							<pattern>HH:mm</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem>MM-dd</dateFormatItem>
						<dateFormatItem>d. MMMM yyyy</dateFormatItem>
						<dateFormatItem>d. MMM. yyyy</dateFormatItem>
						<dateFormatItem>EEEE,d MMMM yyyy</dateFormatItem>
						<dateFormatItem>EEE d MMMM yyyy</dateFormatItem>
						<dateFormatItem>EEE d MMM yy</dateFormatItem>
						<dateFormatItem>d MMMM yyyy</dateFormatItem>
						<dateFormatItem>d MMM yyyy</dateFormatItem>
						<dateFormatItem>d MMM yy</dateFormatItem>
						<dateFormatItem>dd/MM/yy</dateFormatItem>
						<dateFormatItem>dd/MM/yyyy</dateFormatItem>
						<dateFormatItem>QQQQ yy</dateFormatItem>
						<dateFormatItem>MMMM</dateFormatItem>
						<dateFormatItem>dd/MMM</dateFormatItem>
						<dateFormatItem>MM/yy</dateFormatItem>
						<dateFormatItem>EEE dd/MMM yy</dateFormatItem>
						<dateFormatItem>d/MM/yy</dateFormatItem>
						<dateFormatItem>ww</dateFormatItem>
						<dateFormatItem>yyyy-MM-dd</dateFormatItem>
						<dateFormatItem>yy-MM-dd</dateFormatItem>
						<dateFormatItem>mm:ss,SS</dateFormatItem>
						<dateFormatItem>KK:mm:ss a</dateFormatItem>
						<dateFormatItem>KK:mm a</dateFormatItem>
						<dateFormatItem>HH:mm:ss</dateFormatItem>
						<dateFormatItem>HH:mm</dateFormatItem>
						<dateFormatItem>dd/MM/yyyy HH:mm:ss</dateFormatItem>
						<dateFormatItem>dd/MM/yy HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>

