<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="en"/>
		<territory type="ZA"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="full">
						<dateFormat>
							<pattern>EEEE dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>dd MMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>yyyy/MM/dd</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern>h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem>MM-dd</dateFormatItem>
						<dateFormatItem>d. MMMM yyyy</dateFormatItem>
						<dateFormatItem>d. MMM. yyyy</dateFormatItem>
						<dateFormatItem>EEEE,d MMMM yyyy</dateFormatItem>
						<dateFormatItem>EEE d MMMM yyyy</dateFormatItem>
						<dateFormatItem>EEE d MMM yy</dateFormatItem>
						<dateFormatItem>d MMMM yyyy</dateFormatItem>
						<dateFormatItem>d MMM yyyy</dateFormatItem>
						<dateFormatItem>d MMM yy</dateFormatItem>
						<dateFormatItem>dd MMMM yyyy</dateFormatItem>
						<dateFormatItem>yy/MM/dd</dateFormatItem>
						<dateFormatItem>yyyy/MM/dd</dateFormatItem>
						<dateFormatItem>QQQQ yy</dateFormatItem>
						<dateFormatItem>MMMM</dateFormatItem>
						<dateFormatItem>MMM dd</dateFormatItem>
						<dateFormatItem>yy/MM</dateFormatItem>
						<dateFormatItem>EEE dd/MMM yy</dateFormatItem>
						<dateFormatItem>ww</dateFormatItem>
						<dateFormatItem>yyyy-MM-dd</dateFormatItem>
						<dateFormatItem>yy-MM-dd</dateFormatItem>
						<dateFormatItem>mm:ss.SS</dateFormatItem>
						<dateFormatItem>KK:mm:ss a</dateFormatItem>
						<dateFormatItem>KK:mm a</dateFormatItem>
						<dateFormatItem>HH:mm:ss</dateFormatItem>
						<dateFormatItem>HH:mm</dateFormatItem>
						<dateFormatItem>yyyy/MM/dd HH:mm:ss</dateFormatItem>
						<dateFormatItem>yy/MM/dd HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>

