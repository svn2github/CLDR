<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="ro"/>
    <country id="RO"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">ianuarie</month>
        <month id="2">februarie</month>
        <month id="3">martie</month>
        <month id="4">aprilie</month>
        <month id="5">mai</month>
        <month id="6">iunie</month>
        <month id="7">iulie</month>
        <month id="8">august</month>
        <month id="9">septembrie</month>
        <month id="10">octombrie</month>
        <month id="11">noiembrie</month>
        <month id="12">decembrie</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">ian</month>
        <month id="2">feb</month>
        <month id="3">mar</month>
        <month id="4">apr</month>
        <month id="5">mai</month>
        <month id="6">iun</month>
        <month id="7">iul</month>
        <month id="8">aug</month>
        <month id="9">sep</month>
        <month id="10">oct</month>
        <month id="11">noi</month>
        <month id="12">dec</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">duminic&#x0103;</day>
        <day id="2">luni</day>
        <day id="3">mar&#x0163;i</day>
        <day id="4">miercuri</day>
        <day id="5">joi</day>
        <day id="6">vineri</day>
        <day id="7">s&#x00EE;mb&#x0103;t&#x0103;</day>
      </dayNames>

      <dayAbbr>
        <day id="1">d</day>
        <day id="2">l</day>
        <day id="3">ma</day>
        <day id="4">mi</day>
        <day id="5">j</day>
        <day id="6">v</day>
        <day id="7">s</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE, d MMMM yyyy</full>
          <long>d MMMM yyyy</long>
          <short>d.M.yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="ROL">
      <symbol>lei</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>Nu</no>
      <yes>Da</yes>
      <shortNo>Nu</shortNo>
      <shortYes>Da</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
