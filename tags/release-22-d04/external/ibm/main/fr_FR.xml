<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="fr"/>
    <country id="FR"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">janvier</month>
        <month id="2">f&#x00e9;vrier</month>
        <month id="3">mars</month>
        <month id="4">avril</month>
        <month id="5">mai</month>
        <month id="6">juin</month>
        <month id="7">juillet</month>
        <month id="8">ao&#x00fb;t</month>
        <month id="9">septembre</month>
        <month id="10">octobre</month>
        <month id="11">novembre</month>
        <month id="12">d&#x00e9;cembre</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">janv</month>
        <month id="2">f&#x00e9;vr</month>
        <month id="3">mars</month>
        <month id="4">avr</month>
        <month id="5">mai</month>
        <month id="6">juin</month>
        <month id="7">juil</month>
        <month id="8">ao&#x00fb;t</month>
        <month id="9">sept</month>
        <month id="10">oct</month>
        <month id="11">nov</month>
        <month id="12">d&#x00e9;c</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">dimanche</day>
        <day id="2">lundi</day>
        <day id="3">mardi</day>
        <day id="4">mercredi</day>
        <day id="5">jeudi</day>
        <day id="6">vendredi</day>
        <day id="7">samedi</day>
      </dayNames>

      <dayAbbr>
        <day id="1">dim</day>
        <day id="2">lun</day>
        <day id="3">mar</day>
        <day id="4">mer</day>
        <day id="5">jeu</day>
        <day id="6">ven</day>
        <day id="7">sam</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE d MMMM yyyy</full>
          <long>d MMMM yyyy</long>
          <short>dd/MM/yy</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>&#x0020;</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="EUR">
      <symbol>&#x20AC;</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>&#x0020;</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>non</no>
      <yes>oui</yes>
      <shortNo>n</shortNo>
      <shortYes>o</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>00</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
