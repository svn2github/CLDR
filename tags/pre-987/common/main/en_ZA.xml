<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="en"/>
		<territory type="ZA"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="full">
						<dateFormat>
							<pattern>EEEE dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>dd MMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>yyyy/MM/dd</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern>h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem id="1">MM-dd</dateFormatItem>
						<dateFormatItem id="2">d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="3">d. MMM. yyyy</dateFormatItem>
						<dateFormatItem id="4">EEEE,d MMMM yyyy</dateFormatItem>
						<dateFormatItem id="5">EEE d MMMM yyyy</dateFormatItem>
						<dateFormatItem id="6">EEE d MMM yy</dateFormatItem>
						<dateFormatItem id="7">d MMMM yyyy</dateFormatItem>
						<dateFormatItem id="8">d MMM yyyy</dateFormatItem>
						<dateFormatItem id="9">d MMM yy</dateFormatItem>
						<dateFormatItem id="10">dd MMMM yyyy</dateFormatItem>
						<dateFormatItem id="11">yy/MM/dd</dateFormatItem>
						<dateFormatItem id="12">yyyy/MM/dd</dateFormatItem>
						<dateFormatItem id="13">QQQQ yy</dateFormatItem>
						<dateFormatItem id="14">MMMM</dateFormatItem>
						<dateFormatItem id="15">MMM dd</dateFormatItem>
						<dateFormatItem id="16">yy/MM</dateFormatItem>
						<dateFormatItem id="17">EEE dd/MMM yy</dateFormatItem>
						<dateFormatItem id="18">ww</dateFormatItem>
						<dateFormatItem id="19">yyyy-MM-dd</dateFormatItem>
						<dateFormatItem id="20">yy-MM-dd</dateFormatItem>
						<dateFormatItem id="21">mm:ss.SS</dateFormatItem>
						<dateFormatItem id="22">KK:mm:ss a</dateFormatItem>
						<dateFormatItem id="23">KK:mm a</dateFormatItem>
						<dateFormatItem id="24">HH:mm:ss</dateFormatItem>
						<dateFormatItem id="25">HH:mm</dateFormatItem>
						<dateFormatItem id="26">yyyy/MM/dd HH:mm:ss</dateFormatItem>
						<dateFormatItem id="27">yy/MM/dd HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>

