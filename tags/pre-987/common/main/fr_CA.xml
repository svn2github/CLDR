<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="fr"/>
		<territory type="CA"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<week>
					<firstDay day="sun"/>
				</week>
				<dateFormats>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>yy-MM-dd</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>yy-MM-dd</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>HH' h 'mm' min 'ss' s 'z</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem id="1">MM-dd</dateFormatItem>
						<dateFormatItem id="2">d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="3">d. MMM yyyy</dateFormatItem>
						<dateFormatItem id="4">EEEE,d MMMM, yyyy</dateFormatItem>
						<dateFormatItem id="5">EEE d MMMM, yyyy</dateFormatItem>
						<dateFormatItem id="6">EEE d MMM, yy</dateFormatItem>
						<dateFormatItem id="7">d MMMM, yyyy</dateFormatItem>
						<dateFormatItem id="8">d MMM, yyyy</dateFormatItem>
						<dateFormatItem id="9">d MMM, yy</dateFormatItem>
						<dateFormatItem id="10">yy-MM-dd</dateFormatItem>
						<dateFormatItem id="11">yyyy-MM-dd</dateFormatItem>
						<dateFormatItem id="12">QQQQ yy</dateFormatItem>
						<dateFormatItem id="13">MMMM</dateFormatItem>
						<dateFormatItem id="14">MMM dd</dateFormatItem>
						<dateFormatItem id="15">yy-MM</dateFormatItem>
						<dateFormatItem id="16">EEE dd-MMM yy</dateFormatItem>
						<dateFormatItem id="17">ww</dateFormatItem>
						<dateFormatItem id="18">mm:ss,SS</dateFormatItem>
						<dateFormatItem id="19">KK:mm:ss a</dateFormatItem>
						<dateFormatItem id="20">KK:mm a</dateFormatItem>
						<dateFormatItem id="21">HH:mm:ss</dateFormatItem>
						<dateFormatItem id="22">HH:mm</dateFormatItem>
						<dateFormatItem id="23">yyyy-MM-dd HH:mm:ss</dateFormatItem>
						<dateFormatItem id="24">yy-MM-dd HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>#,##0.00 ¤;(#,##0.00¤)</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
		<currencies>
			<currency type="CAD">
				<symbol>$</symbol>
			</currency>
			<currency type="USD">
				<symbol>$ US</symbol>
			</currency>
		</currencies>
	</numbers>
</ldml>

