<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="de"/>
		<territory type="LU"/>
	</identity>
	<numbers>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
		<currencies>
			<currency type="LUF">
				<pattern>#,##0 ¤;-#,##0 ¤</pattern>
				<symbol>F</symbol>
				<decimal>.</decimal>
				<group>,</group>
			</currency>
		</currencies>
	</numbers>
</ldml>

