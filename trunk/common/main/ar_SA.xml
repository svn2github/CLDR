<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="ar"/>
		<territory type="SA"/>
	</identity>
	<numbers>
		<defaultNumberingSystem>arab</defaultNumberingSystem>
		<otherNumberingSystems>
			<native>arab</native>
		</otherNumberingSystems>
		<symbols numberSystem="arab">
			<decimal>٫</decimal>
			<group>٬</group>
			<percentSign>٪</percentSign>
			<plusSign>+</plusSign>
			<perMille>؉</perMille>
		</symbols>
		<symbols numberSystem="latn">
			<decimal>.</decimal>
			<group>,</group>
			<percentSign>%</percentSign>
			<plusSign>+</plusSign>
			<perMille>‰</perMille>
		</symbols>
		<decimalFormats numberSystem="latn">
			<decimalFormatLength>
				<decimalFormat>
					<pattern>#0.###;#0.###-</pattern>
				</decimalFormat>
			</decimalFormatLength>
		</decimalFormats>
		<currencyFormats numberSystem="latn">
			<currencyFormatLength>
				<currencyFormat>
					<pattern>¤#0.00</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>
