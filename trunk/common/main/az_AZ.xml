<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml draft="true">
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="az"/>
		<territory type="AZ"/>
	</identity>
	<!-- default script for Azeri is Latin -->
	<alias source="az" path="//ldml"/>
</ldml>

