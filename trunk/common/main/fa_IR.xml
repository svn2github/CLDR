<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="fa"/>
		<territory type="IR"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<week>
					<firstDay day="sat"/>
				</week>
			</calendar>
			<calendar type="islamic">
				<week>
					<minDays count="1"/>
					<firstDay day="sat" draft="unconfirmed"/>
				</week>
			</calendar>
			<calendar type="persian">
				<week>
					<minDays count="1"/>
					<firstDay day="sat" draft="unconfirmed"/>
				</week>
			</calendar>
		</calendars>
	</dates>
</ldml>
