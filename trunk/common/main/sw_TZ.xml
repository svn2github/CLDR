<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "	http://oss.software.ibm.com/cvs/icu/~checkout~/locale/ldml.dtd">
<ldml>
	<identity>
		<version number="1.2"/>
		<generation date="2004-08-27" />
		<language type="sw" /> 
		<territory type="TZ" /> 
	</identity>
		<numbers>
			<decimalFormats>
				<decimalFormatLength >
					<decimalFormat >
						<pattern>#,##0.###;-#,##0.###</pattern>
					</decimalFormat>
				</decimalFormatLength >
			</decimalFormats>
			<scientificFormats>
				<scientificFormatLength >
					<scientificFormat >
						<pattern>#E0</pattern>
					</scientificFormat>
				</scientificFormatLength >
			</scientificFormats>
			<percentFormats>
				<percentFormatLength >
					<percentFormat >
						<pattern>#,##0%</pattern>
					</percentFormat>
				</percentFormatLength >
			</percentFormats>
			<currencyFormats>
				<currencyFormatLength >
					<currencyFormat >
						<pattern>#,##0.00 ¤;-#,##0.00 ¤</pattern>
					</currencyFormat>
				</currencyFormatLength >
			</currencyFormats>
		</numbers>
</ldml>

