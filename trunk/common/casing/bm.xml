<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision: $"/>
		<generation date="$Date: $"/>
		<language type="bm"/>
	</identity>
	<behavior>
		<casing type="calendar-field">lowercase</casing>
		<casing type="day-format-except-narrow">lowercase</casing>
		<casing type="day-narrow">uppercase</casing>
		<casing type="day-standalone-except-narrow">lowercase</casing>
		<casing type="displayName">lowercase</casing>
		<casing type="era-name">lowercase</casing>
		<casing type="language">lowercase</casing>
		<casing type="month-format-except-narrow">lowercase</casing>
		<casing type="month-narrow">uppercase</casing>
		<casing type="month-standalone-except-narrow">lowercase</casing>
		<casing type="quarter-abbreviated">uppercase</casing>
		<casing type="quarter-format-wide">lowercase</casing>
		<casing type="quarter-standalone-wide">lowercase</casing>
		<casing type="tense">lowercase</casing>
		<casing type="territory">uppercase</casing>
	</behavior>
</ldml>
