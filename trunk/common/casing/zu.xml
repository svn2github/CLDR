<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision: $"/>
		<generation date="$Date: $"/>
		<language type="zu"/>
	</identity>
	<behavior>
		<casing type="calendar-field">uppercase</casing>
		<casing type="day-format-except-narrow">uppercase</casing>
		<casing type="day-narrow">uppercase</casing>
		<casing type="day-standalone-except-narrow">uppercase</casing>
		<casing type="displayName">lowercase</casing>
		<casing type="displayName-count">lowercase</casing>
		<casing type="era-abbr">uppercase</casing>
		<casing type="era-name">uppercase</casing>
		<casing type="era-narrow">uppercase</casing>
		<casing type="language">lowercase</casing>
		<casing type="metazone-long">lowercase</casing>
		<casing type="month-format-except-narrow">uppercase</casing>
		<casing type="month-narrow">uppercase</casing>
		<casing type="quarter-abbreviated">uppercase</casing>
		<casing type="quarter-format-wide">lowercase</casing>
		<casing type="quarter-standalone-wide">lowercase</casing>
		<casing type="script">lowercase</casing>
		<casing type="symbol">uppercase</casing>
		<casing type="tense">uppercase</casing>
		<casing type="territory">lowercase</casing>
		<casing type="type">lowercase</casing>
	</behavior>
</ldml>
