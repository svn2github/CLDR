<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision: $"/>
		<generation date="$Date: $"/>
		<language type="vai"/>
		<script type="Latn"/>
	</identity>
	<behavior>
		<casing type="calendar-field">lowercase</casing>
		<casing type="day-format-except-narrow">lowercase</casing>
		<casing type="day-standalone-except-narrow">lowercase</casing>
		<casing type="displayName">uppercase</casing>
		<casing type="language">uppercase</casing>
		<casing type="month-format-except-narrow">lowercase</casing>
		<casing type="month-standalone-except-narrow">lowercase</casing>
		<casing type="tense">lowercase</casing>
		<casing type="territory">uppercase</casing>
	</behavior>
</ldml>
