<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision: $"/>
		<generation date="$Date: $"/>
		<language type="it"/>
	</identity>
	<behavior>
		<casing type="calendar-field">lowercase</casing>
		<casing type="day-format-except-narrow">lowercase</casing>
		<casing type="day-narrow">uppercase</casing>
		<casing type="displayName">uppercase</casing>
		<casing type="language">lowercase</casing>
		<casing type="metazone-long">uppercase</casing>
		<casing type="metazone-short">uppercase</casing>
		<casing type="month-narrow">uppercase</casing>
		<casing type="quarter-abbreviated">uppercase</casing>
		<casing type="script">lowercase</casing>
		<casing type="symbol">uppercase</casing>
		<casing type="territory">uppercase</casing>
		<casing type="type">uppercase</casing>
		<casing type="variant">lowercase</casing>
		<casing type="zone-exemplarCity">uppercase</casing>
	</behavior>
</ldml>
