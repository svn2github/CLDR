<ldml>
    <rbnf>
        <ruleset type="ordinal">
            <rbnfrule value="0">第=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting">
            <rbnfrule value="-x">負→→;</rbnfrule>
            <rbnfrule value="x.x">←←點→→→;</rbnfrule>
            <rbnfrule value="0">零;</rbnfrule>
            <rbnfrule value="1">壹;</rbnfrule>
            <rbnfrule value="2">貳;</rbnfrule>
            <rbnfrule value="3">叄;</rbnfrule>
            <rbnfrule value="4">肆;</rbnfrule>
            <rbnfrule value="5">伍;</rbnfrule>
            <rbnfrule value="6">陸;</rbnfrule>
            <rbnfrule value="7">柒;</rbnfrule>
            <rbnfrule value="8">捌;</rbnfrule>
            <rbnfrule value="9">玖;</rbnfrule>
            <rbnfrule value="10">拾[→→];</rbnfrule>
            <rbnfrule value="20">=%%checkwriting1=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting1" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="10">←←拾[→→];</rbnfrule>
            <rbnfrule value="100">←←佰[→%%checkwriting2→];</rbnfrule>
            <rbnfrule value="1000">←←仟[→%%checkwriting3→];</rbnfrule>
            <rbnfrule value="10000">←←萬[→%%checkwriting4→];</rbnfrule>
            <rbnfrule value="100000000">←←億[→%%checkwriting8→];</rbnfrule>
            <rbnfrule value="1000000000000">←←兆[→%%checkwriting12→];</rbnfrule>
            <rbnfrule value="10000000000000000">←←京[→%%checkwriting16→];</rbnfrule>
            <rbnfrule value="100000000000000000000">=#,###0.#=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting2" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="10">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting3" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="100">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting4" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="1000">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting8" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="10000000">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting12" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="100000000000">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="checkwriting16" access="private">
            <rbnfrule value="0">=%checkwriting=;</rbnfrule>
            <rbnfrule value="1">零=%checkwriting=;</rbnfrule>
            <rbnfrule value="1000000000000000">=%checkwriting=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout">
            <rbnfrule value="-x">負→→;</rbnfrule>
            <rbnfrule value="x.x">←←點→→→;</rbnfrule>
            <rbnfrule value="0">〇;</rbnfrule>
            <rbnfrule value="1">一;</rbnfrule>
            <rbnfrule value="2">二;</rbnfrule>
            <rbnfrule value="3">三;</rbnfrule>
            <rbnfrule value="4">四;</rbnfrule>
            <rbnfrule value="5">五;</rbnfrule>
            <rbnfrule value="6">六;</rbnfrule>
            <rbnfrule value="7">七;</rbnfrule>
            <rbnfrule value="8">八;</rbnfrule>
            <rbnfrule value="9">九;</rbnfrule>
            <rbnfrule value="10">十[→→];</rbnfrule>
            <rbnfrule value="20">←←十[→→];</rbnfrule>
            <rbnfrule value="100">百[→%%spellout2→];</rbnfrule>
            <rbnfrule value="200">←←百[→%%spellout2→];</rbnfrule>
            <rbnfrule value="1000">=%%spellout1=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout1" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="10">←←十[→→];</rbnfrule>
            <rbnfrule value="100">←←百[→%%spellout2→];</rbnfrule>
            <rbnfrule value="1000">←←千[→%%spellout3→];</rbnfrule>
            <rbnfrule value="10000">←←萬[→%%spellout4→];</rbnfrule>
            <rbnfrule value="100000000">←←億[→%%spellout8→];</rbnfrule>
            <rbnfrule value="1000000000000">←←兆[→%%spellout12→];</rbnfrule>
            <rbnfrule value="10000000000000000">←←京[→%%spellout16→];</rbnfrule>
            <rbnfrule value="100000000000000000000">=#,###0.#=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout2" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="10">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout3" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="100">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout4" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="1000">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout8" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="10000000">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout12" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="100000000000">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout16" access="private">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
            <rbnfrule value="1">〇=%spellout=;</rbnfrule>
            <rbnfrule value="1000000000000000">=%spellout=;</rbnfrule>
        </ruleset>
    </rbnf>
</ldml>
