<ldml>
    <rbnf>
        <ruleset type="spellout_year">
            <rbnfrule value="-x">minus →→;</rbnfrule>
            <rbnfrule value="0">null; én; to; tre; fire; fem; seks; syv; åtte; ni;</rbnfrule>
            <rbnfrule value="10">ti; elve; tolv; tretten; fjorten; femten; seksten; sytten; atten; nitten;</rbnfrule>
            <rbnfrule value="20">[→→­og­]tyve;</rbnfrule>
            <rbnfrule value="30">[→→­og­]tredve;</rbnfrule>
            <rbnfrule value="40">[→→­og­]førr;</rbnfrule>
            <rbnfrule value="50">[→→­og­]femti;</rbnfrule>
            <rbnfrule value="60">[→→­og­]seksti;</rbnfrule>
            <rbnfrule value="70">[→→­og­]søtti;</rbnfrule>
            <rbnfrule value="80">[→→­og­]åtti;</rbnfrule>
            <rbnfrule value="90">[→→­og­]nitti;</rbnfrule>
            <rbnfrule value="100">ett­hundre[ og →→];</rbnfrule>
            <rbnfrule value="200">←←­hundre[ og →→];</rbnfrule>
            <rbnfrule value="1000">tusen[ og →→];</rbnfrule>
            <rbnfrule value="1100/100">←←­hundre[ og →→];</rbnfrule>
            <rbnfrule value="10000">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="neuter">
            <rbnfrule value="-x">minus →→;</rbnfrule>
            <rbnfrule value="x.x">←← komma →→;</rbnfrule>
            <rbnfrule value="0">null; et; =%reale=;</rbnfrule>
        </ruleset>
        <ruleset type="masculine">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="feminine">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="spellout">
            <rbnfrule value="-x">minus →→;</rbnfrule>
            <rbnfrule value="x.x">←← komma →→;</rbnfrule>
            <rbnfrule value="0">null; én; to; tre; fire; fem; seks; syv; åtte; ni;</rbnfrule>
            <rbnfrule value="10">ti; elve; tolv; tretten; fjorten; femten; seksten; sytten; atten; nitten;</rbnfrule>
            <rbnfrule value="20">[→→­og­]tyve;</rbnfrule>
            <rbnfrule value="30">[→→­og­]tredve;</rbnfrule>
            <rbnfrule value="40">[→→­og­]førr;</rbnfrule>
            <rbnfrule value="50">[→→­og­]femti;</rbnfrule>
            <rbnfrule value="60">[→→­og­]seksti;</rbnfrule>
            <rbnfrule value="70">[→→­og­]søtti;</rbnfrule>
            <rbnfrule value="80">[→→­og­]åtti;</rbnfrule>
            <rbnfrule value="90">[→→­og­]nitti;</rbnfrule>
            <rbnfrule value="100">ett­hundre[ og →→];</rbnfrule>
            <rbnfrule value="200">←%neutrum←­hundre[ og →→];</rbnfrule>
            <rbnfrule value="1000">←%neutrum← tusen[ og →→];</rbnfrule>
            <rbnfrule value="1000000">én million[ →→];</rbnfrule>
            <rbnfrule value="2000000">←← millioner[ →→];</rbnfrule>
            <rbnfrule value="1000000000">én milliard[ →→];</rbnfrule>
            <rbnfrule value="2000000000">←← milliarder[ →→];</rbnfrule>
            <rbnfrule value="1000000000000">én billion[ →→];</rbnfrule>
            <rbnfrule value="2000000000000">←← billioner[ →→];</rbnfrule>
            <rbnfrule value="1000000000000000">én billiard[ →→];</rbnfrule>
            <rbnfrule value="2000000000000000">←← billiarder[ →→];</rbnfrule>
            <rbnfrule value="1000000000000000000">=#,##0.#=;</rbnfrule>
        </ruleset>
        <ruleset type="ordinal">
            <rbnfrule value="0">=#,##0==%%ordabbrev=;</rbnfrule>
        </ruleset>
        <ruleset type="ordabbrev" access="private">
            <rbnfrule value="0">'.;</rbnfrule>
        </ruleset>
        <ruleset type="amount">
            <rbnfrule value="0">=%spellout=;</rbnfrule>
        </ruleset>
        <ruleset type="default">
            <rbnfrule value="0">=%amount=;</rbnfrule>
        </ruleset>
    </rbnf>
</ldml>
