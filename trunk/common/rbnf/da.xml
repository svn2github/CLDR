            <rbnfrule value="2000000000000000">←← billiarder[ og →→];</rbnfrule>
            <rbnfrule value="1000000000000000000">=#,##0.#=;</rbnfrule>
        </ruleset>
        <ruleset type="ordinal">
            <rbnfrule value="0">=#,##0==%%ordabbrev=;</rbnfrule>
        </ruleset>
        <ruleset type="ordabbrev" access="private">
            <rbnfrule value="0">'.;</rbnfrule>
