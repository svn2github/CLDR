            <rbnfrule value="200">←%neutrum←­hundred[ og →→];</rbnfrule>
            <rbnfrule value="1000">←%neutrum← tusinde[ og →→];</rbnfrule>
            <rbnfrule value="2000">←%neutrum← tusind[ og →→];</rbnfrule>
