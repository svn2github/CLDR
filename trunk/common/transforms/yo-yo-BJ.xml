<supplementalData>
  <version number="$Revision$"/>
	<generation date="$Date$"/>
	<transforms>
		<transform source="yo" target="yo-BJ" direction="forward" variant="BGN" draft="provisional">
			<comment>
			########################################################################
			#
			# Latin Characters that must be changed:
			#
			#   ẸỌṢ
			#   ẹọṣ
			#
			# Originally prepared by Jonathan Lai ( jali01@ca.ibm.com )
			########################################################################
			#
			# MINIMAL FILTER: yo-yo-BJ
			#
			</comment>
			<tRule>:: [ẸỌṢẹọṣ] ;</tRule>
			<tRule>:: NFD (NFC) ;</tRule>
			<comment>
			#
			########################################################################
			</comment>

			<comment>
			########################################################################
			#
			# Start of Alphabetic Transformations
			#
			########################################################################
			#
			</comment>
			<tRule>Ẹ → Ɛ ; # LATIN CAPITAL LETTER A</tRule>
			<tRule>ẹ → ɛ ; # LATIN SMALL LETTER A</tRule>
			<tRule>Ọ → Ɔ ; # LATIN CAPITAL LETTER BE</tRule>
			<tRule>ọ → ɔ ; # LATIN SMALL LETTER BE</tRule>
			<tRule>Ṣ → Sh ; # LATIN CAPITAL LETTER VE</tRule>
			<tRule>ṣ → sh ; # LATIN SMALL LETTER VE</tRule>
			<comment>
			#
			########################################################################
			</comment>
		</transform>
	</transforms>
</supplementalData>
