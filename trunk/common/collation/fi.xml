					<x><s>þ</s><extend>h</extend></x>
					<x><t>Þ</t><extend>h</extend></x>
					<reset>V</reset>
					<s>w</s>
					<t>W</t>
					<reset>Y</reset>
					<s>ü</s>
					<t>Ü</t>
