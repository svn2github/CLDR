				<rules>
					<!-- D and U+0335 -->
					<reset>D ̵</reset>
					<s>đ</s>
					<t>Đ</t>
					<!-- G and U+0335 -->
					<reset>G ̵</reset>
					<s>ǥ</s>
					<t>Ǥ</t>
					<!-- N and U+0335 -->
					<reset>N ̵</reset>
					<s>ŋ</s>
					<t>Ŋ</t>
