<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="be"/> 
	</identity>
	<collations  validSubLocales="be_BY">
		<collation type="standard">
			<settings reorder="Cyrl"/>
			<suppress_contractions>[АаӘәГгЕеЖжЗзІіОоӨөКкЧчЫыЭэѴѵ]</suppress_contractions>
			<!--
				List of UCA contractions remaining after suppression:

				Ё 0415 0308 CYRILLIC CAPITAL LETTER IO
				ё 0435 0308 CYRILLIC SMALL LETTER IO
				Й 0418 0306 CYRILLIC CAPITAL LETTER SHORT I
				й 0438 0306 CYRILLIC SMALL LETTER SHORT I
				Ў 0423 0306 CYRILLIC CAPITAL LETTER SHORT U
				ў 0443 0306 CYRILLIC SMALL LETTER SHORT U
			-->
			<rules>
			    <reset>Е</reset>
			        <p>ё</p>
			        <t>Ё</t>
			</rules>
		</collation>
	</collations>
</ldml>
