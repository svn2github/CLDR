<!-- Uyghur letter names in comments are written in Latin-Script Uyghur alphabet -->
<ldml>
  <identity>
    <version number="$Revision$"/>
    <generation date="$Date$"/>
    <language type="ug"/>
  </identity>
  <collations>
    <collation type="standard" draft="unconfirmed">
      <rules>
	<reset>ا</reset>
	<!-- E is sorted after A, position 2-->
	<p>ە</p>
	<!-- B is sorted after E, position 3-->
	<p>ب</p>
	<reset>ك</reset>
	<!-- G is sorted after K, position 19-->
	<p>گ</p>
	<!-- NG is sorted after G, position 20-->
	<p>ڭ</p>
	<!-- L is sorted after NG, position 21-->
	<p>ل</p>
	<reset>ھ</reset>
	<!-- O is sorted after H, position 25-->
	<p>و</p>
	<!-- U is sorted after O, position 26-->
	<p>ۇ</p>
	<!-- Ö is sorted after U, position 27-->
	<p>ۆ</p>
	<!-- Ü is sorted after Ö, position 28-->
	<p>ۈ</p>
	<!-- W is sorted after Ü, position 29-->
	<p>ۋ</p>
	<!-- é is sorted after W, position 30-->
	<p>ې</p>
	<!-- I is sorted after é, position 31-->
	<p>ى</p>
	<!-- Y is sorted after I, position 32-->
	<p>ي</p>
      </rules>
    </collation>
  </collations>
</ldml>
