<?xml version="1.0" encoding="UTF-8" ?>
<!--add CoreLocaleElements DTD reference here-->
<!-- Tag was "CoreLocaleData" - this has been changed below to allow the
Cupertino XML tool to work -->
<!DOCTYPE localeData SYSTEM "./LocaleElements.dtd" >
<localeData>
<generation date="2002, June, 12"></generation>
<!--
Core Locale Data Format
Version 1.0.4: 2002-05-23
	
Contains locale data elements from ICIR (version 1.4.1) that map into 
CoreLocaleElements.xml.
-->

<!--ICIR VERSION information-->
  <versioning>
    <version number="1.4.1"></version>
  </versioning>

<!--IDENTIFICATION of the locale-->
  <identity>
    <!--Two letter ISO codes only-->
    <language id="hr"/>
    <country id="HR"/>
  </identity>

<!--CALENDAR INFORMATION.  Gregorian Only-->
  <calendars>
    <calendar class="Gregorian" default="true">
      <monthNames>
        <month id="1">sije&#x0109;anj</month>
        <month id="2">belja&#x0109;a</month>
        <month id="3">o&#x017e;ujak</month>
        <month id="4">travanj</month>
        <month id="5">svibanj</month>
        <month id="6">lipanj</month>
        <month id="7">srpanj</month>
        <month id="8">kolovoz</month>
        <month id="9">rujan</month>
        <month id="10">listopad</month>
        <month id="11">studeni</month>
        <month id="12">prosinac</month>
        <month id="13"></month>
      </monthNames>

      <monthAbbr>
        <month id="1">sij</month>
        <month id="2">vel</month>
        <month id="3">o&#x017e;u</month>
        <month id="4">tra</month>
        <month id="5">svi</month>
        <month id="6">lip</month>
        <month id="7">srp</month>
        <month id="8">kol</month>
        <month id="9">ruj</month>
        <month id="10">lis</month>
        <month id="11">stu</month>
        <month id="12">pro</month>
        <month id="13"></month>
      </monthAbbr>

      <dayNames>
        <day id="1">nedjelja</day>
        <day id="2">ponedjeljak</day>
        <day id="3">utorak</day>
        <day id="4">srijeda</day>
        <day id="5">&#x010D;etvrtak</day>
        <day id="6">petak</day>
        <day id="7">subota</day>
      </dayNames>

      <dayAbbr>
        <day id="1">ned</day>
        <day id="2">pon</day>
        <day id="3">uto</day>
        <day id="4">sri</day>
        <day id="5">&#x010D;et</day>
        <day id="6">pet</day>
        <day id="7">sub</day>
      </dayAbbr>

      <!--
      Patterns used for formatting and parsing dates and times for this calendar
      -->

      <patterns>
        <!-- <chars>GyMdhHmsSEDFwWakKz</chars> -->
        <time>
          <full>HH:mm:ss</full>
          <medium>HH:mm:ss</medium>
          <short>HH:mm</short>
        </time>

        <date>
          <full>EEEE, dd. MMMM yyyy.</full>
          <long>dd. MMMM yyyy.</long>
          <short>yy-MM-dd</short>
        </date>
      </patterns>
    </calendar>
  </calendars>

<!--NUMERIC INFORMATION-->
  <numberFormat class="decimal" default="true">
    <patterns>
      <decimal>#,###,##0.##;-#,###,##0.##</decimal>
    </patterns>
    <symbols>
      <decimal>,</decimal>
      <group>.</group>
      <minusSign>-</minusSign>
    </symbols>
  </numberFormat>

<!--MONETARY INFORMATION-->
  <currencies>
    <!--currency id as per ISO4217, three letter alphabetic code-->
    <currency id="HRK">
      <symbol>kn</symbol>
      <pattern>#,###,##0.00 &#x00A4;;-#,###,##0.00 &#x00A4;</pattern>
      <decimal>,</decimal>
      <group>.</group>
    </currency>
  </currencies>

<!--POSIX TYPE INFORMATION-->
  <PosixCompatibilityData>
    <!--POSIX LC_MESSAGE EQUIVALENT-->
    <messages>
      <no>Ne</no>
      <yes>Da</yes>
      <shortNo>N</shortNo>
      <shortYes>D</shortYes>
    </messages>

    <!--TELEPHONE INFORMATION-->
    <TelephoneFormat>
      <!-- International Access Code is shown below -->
      <InternationalDialCode>99</InternationalDialCode>
    </TelephoneFormat>
  </PosixCompatibilityData>

<!-- Tag was "/CoreLocaleData" - changed below to allow Cupertino XML
tool to work -->
</localeData>
