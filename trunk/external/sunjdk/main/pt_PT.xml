<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.openi18n.org/spec/ldml/1.0/ldml.dtd">
<ldml>
	<identity>
		<version number="1.0"/>
		<generation date="2003-12-19" />
		<language type="pt" /> 
		<territory type="PT" /> 
	</identity>
	<numbers>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##0%</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0.00 ¤;-#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
		<currencies>
			<default type="EUR"/>
			<currency type="EUR">
				<decimal>$</decimal>
			</currency>
		</currencies>
	</numbers>
</ldml>
