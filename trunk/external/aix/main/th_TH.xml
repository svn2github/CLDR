					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="abbreviated">
							<day type="sun">อา</day>
							<day type="mon"> จ</day>
							<day type="tue"> อ</day>
							<day type="wed"> พ</day>
							<day type="thu">พฤ</day>
