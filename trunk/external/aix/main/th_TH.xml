							<month type="10">ต.ค. </month>
							<month type="11">พ.ย. </month>
							<month type="12">ธ.ค. </month>
							<month type="13"></month>
						</monthWidth>
						<monthWidth type="wide">
							<month type="1">มกราคม</month>
							<month type="2">กุมภาพันธ์</month>
