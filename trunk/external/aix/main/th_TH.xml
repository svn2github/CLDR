					<dateFormatLength type="full">
						<dateFormat >
							<pattern>dd MMM %o</pattern>
						</dateFormat>
					</dateFormatLength >
					<dateFormatLength type="long">
						<dateFormat >
							<pattern>EEE dd MMM %o h:mm a</pattern>
						</dateFormat>
