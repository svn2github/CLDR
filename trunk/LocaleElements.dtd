<!-- RBNF data -->
<!-- Either 1 alias OR any specials, any order, zero or more ruleBasedNumberFormat -->
<!ELEMENT ruleBasedNumberFormats ( alias | (ruleBasedNumberFormat | special)*) >

<!-- Either bare data or any number of cp -->
<!ELEMENT ruleBasedNumberFormat (#PCDATA | cp)* >
<!ATTLIST ruleBasedNumberFormat type NMTOKEN #REQUIRED >

<!-- RBBI data -->
<!-- should be : (alias | (grapheme?, word?, line?, sentence?)) except for ordering and specials -->
<!ELEMENT boundaries (alias | (grapheme | word | line | sentence | special)*) >

<!ELEMENT grapheme ( #PCDATA ) >
<!ATTLIST grapheme class NMTOKEN #REQUIRED >
<!ATTLIST grapheme append NMTOKEN #REQUIRED >

<!ELEMENT word ( #PCDATA ) >
<!ATTLIST word class NMTOKEN #REQUIRED >
<!ATTLIST word import NMTOKEN #REQUIRED >

<!ELEMENT line ( #PCDATA ) >
<!ATTLIST line class NMTOKEN #REQUIRED >


<!ELEMENT sentence ( #PCDATA ) >
<!ATTLIST sentence class NMTOKEN #REQUIRED >

<!-- RBT data  -->
<!-- Either 1 alias OR any specials, any order, zero or more transform -->
<!ELEMENT transforms ( alias | (transform | special)*) >
<!ELEMENT transform ( #PCDATA ) >
<!ATTLIST transform type NMTOKEN #REQUIRED >

