<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE LocaleElements SYSTEM "./LocaleElements.dtd" >
<localeData>
<!--
Locale Data Format
Draft: 2000-06-16

Format that contains the required locale data for use by Java, ICU and POSIX APIs.
Note: constructing a POSIX locale also requires use of the Unicode Character Database data.
That data being common across locales, it is not duplicated here.
See ftp://ftp.unicode.org/Public/UNIDATA/UnicodeCharacterDatabase.html.

NOTE: The data below is purely illustrative, and doesn't match any particular language.
NOTE: The data for any given locale will only contain items that are different from the parent locale.
For example, most country locales will inherit the bulk of their data from the language locale:
"en" will contain the bulk of the data: en_US will only contain a few items like currency.
-->
     <identity>
        <generation date="2002-08-28" />
        <versioning>
           <version number="1.1">Various notes and changes in version 1.1</version>
           <version number="1.0">Various notes and changes in version 1.0</version>
        </versioning>

        <!-- Two letter codes, if available; otherwise 3-letter codes.If both exist, a separate database contains the mapping. -->
        <language id="en" name="english"/>
        <country id="US" name="United States"/>

        <!-- Only one variant allowed for now -->
        <variant id="B"/>
        <correspondsTo vendor="windows"/>
        <special owner="windows" lcid="0409"/>
     </identity>
     <layout>
        <orientation lines="top-to-bottom" characters="left-to-right" />    <!-- both attributes are required -->
     </layout>
     <encodings requiredLetters="a-z&#xE5;&#xE6;&#xF8;">    <!-- optional attribute -->
        <!--
        List of encodings that are in common use in this locale.
        The order is not significant.
        The format is as in http://www.unicode.org/unicode/reports/tr22/
        with the version suppressed.
        (It is assumed that the largest available version is chosen.)
        The required letters can also be used to select appropriate encodings.
        -->
       <mapping>windows-1252</mapping>
       <mapping>iso-8859_1</mapping>
       <mapping>euc-jp</mapping>
     </encodings>

    <displayNames>
        <!--
        Display names for scripts, languages, countries, and variants in this locale
        -->
        <scripts>
            <!-- IDs come from the Unicode Tech Report on scripts -->
            <script id="Latin">Romana</script>
            <script id="Cyrillic">Kyrillica</script>
         </scripts>

        <languages>
            <language id="ab">Abkhazian</language>
            <language id="aa">Afar</language>
            <language id="af">Afrikaans</language>
            <language id="sq">Albanian</language>
        </languages>

        <countries>
            <country id="AF">Afghanistan</country>
            <country id="AL">Albania</country>
            <country id="DZ">Algeria</country>
            <country id="AD">Andorra</country>
            <country id="AO">Angola</country>
            <country id="US">United States</country>
        </countries>

        <variants>
            <!-- Variant IDs must be unique across ALL locales. Currently they include
            B, NY, EURO -->
            <variant id="B">Bokm&#x00e5;l</variant>
        </variants>

        <localePatterns>
            <!--
                Java/ICU choice format syntax for producing the name of a locale in a particular language.
             -->
            <locale>{0,choice,0#|1#{1}|2#{1} ({2})}</locale>
            <list>{0,choice,0#|1#{1}|2#{1},{2}|3#{1},{2},{3}}</list>
            <composition>{0},{1}</composition>
        </localePatterns>
    </displayNames>

    <dates>
        <!--
        There can be multiple calendars in the same locale, e.g. Gregorian + Imperial Japanese.
        The ids must match what is produced by the calendar system.
        -->
        <calendar class="Gregorian" default="true">
            <monthNames>
                <month id="jan">january</month>
                <month id="feb">february</month>
                <month id="mar">march</month>
                <month id="apr">april</month>
                <month id="may">may</month>
                <month id="jun">june</month>
                <month id="jul">july</month>
                <month id="aug">august</month>
                <month id="sep">September</month>
                <month id="oct">October</month>
                <month id="nov">November</month>
                <month id="dec">December</month>
            </monthNames>
            <monthAbbr>
                <month id="jan">Jan</month>
                <month id="feb">Feb</month>
                <month id="mar">Mar</month>
                <month id="apr">Apr</month>
                <month id="may">May</month>
                <month id="jun">Jun</month>
                <month id="jul">Jul</month>
                <month id="aug">Aug</month>
                <month id="sep">Sep</month>
                <month id="oct">Oct</month>
                <month id="nov">Nov</month>
                <month id="dec">Dec</month>
            </monthAbbr>

            <dayNames>
                <day id="sun">Sunday</day>
                <day id="mon">Monday</day>
                <day id="tue">Tuesday</day>
                <day id="wed">Wednesday</day>
                <day id="thu">Thursday</day>
                <day id="fri">Friday</day>
                <day id="sat">Saturday</day>
            </dayNames>
            <dayAbbr>
                <day id="sun">Sun</day>
                <day id="mon">Mon</day>
                <day id="tue">Tue</day>
                <day id="wed">Wed</day>
                <day id="thu">Thu</day>
                <day id="fri">Fri</day>
                <day id="sat">Sat</day>
            </dayAbbr>
            <week>
                <minDays>1</minDays>
                <firstDay id="sun"/>
                <weekendStart>
                    <day id="fri"></day>
                    <time>18:00</time>
                </weekendStart>
                <weekendEnd>
                    <day id="sun"></day>
                    <time>18:00</time>
                </weekendEnd>
            </week>
            <am>AM</am>
            <pm>PM</pm>

             <eras>
                <!-- Non-gregorians may have many more eras -->
                <era id="0">BC</era>
                <era id="1">AD</era>
            </eras>
            <patterns>
               <!--
                        Patterns used for formatting and parsing dates and times for this calendar
                -->
                <localizedPatternChars>GyMdkHmsSEDFwWahKz</localizedPatternChars>
                <patternElement patternid="date" type="medium" default="true">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="StarOffice" msgid="DateFormatskey3" usage="DATE" formatindex="20" />
                      <patternCode >MM d, yyyy </patternCode>
                      <patternName>DIN 5008 (EN 28601)</patternName>
                 </patternElement>
                 <patternElement patternid="date" type="full" default="true">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="StarOffice" msgid="DateFormatskey3" usage="DATE" formatindex="20" />
                      <patternCode>EEEE, MMMM d, yyyy</patternCode>
                      <patternName>DIN 5008 (EN 28601)</patternName>
                 </patternElement>
                  <patternElement patternid="time" type="full" default="true">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="StarOffice" msgid="DateFormatskey3" usage="DATE" formatindex="20" />
                      <patternCode>h:mm:ss a z</patternCode>
                      <patternName>DIN 5008 (EN 28601)</patternName>
                 </patternElement>
                <patternElement patternid="dateTime">{0} {1}</patternElement>

            </patterns>
        </calendar>

        <!-- The Buddhist calendar inherits month names, etc. (everything but eras) from Gregorian -->
        <calendar class="Buddhist" inheritsFrom="Gregorian">
            <eras>
                <era id="0">BE</era>
            </eras>
        </calendar>
        <timeZoneNames>
                <!--
                The zone id's are language-independent.
                Their values and data (default offset from GMT, daylight savings rules)
                are stored elsewhere
                Here we just have the localized names for them.
                -->
                <zone id="America/Los_Angeles" default="true">
                    <long>
                        <generic>Pacific Time</generic>
                        <standard>Pacific Standard Time</standard>
                        <daylight>Pacific Daylight Time</daylight>
                    </long>
                    <short>
                        <generic>PT</generic>
                        <standard>PST</standard>
                        <daylight>PDT</daylight>
                    </short>
                    <exemplarCity>Los Angeles</exemplarCity>
                </zone>

                <zone id="America/Denver">
                    <long>
                        <generic>Mountain Time</generic>
                        <standard>Mountain Standard Time</standard>
                        <daylight>Mountain Daylight Time</daylight>
                    </long>
                    <short>
                        <generic>MT</generic>
                        <standard>MST</standard>
                        <daylight>MDT</daylight>
                    </short>
                    <exemplarCity>Denver</exemplarCity>
                </zone>

                <zone id="Europe/London">
                     <long>
                        <generic>British Time</generic>
                        <standard>British Standard Time</standard>
                        <daylight>British Daylight Time</daylight>
                    </long>
                    <exemplarCity>York</exemplarCity>
                </zone>

        </timeZoneNames>
   </dates>

   <delimiters>
       <quotationStart>'</quotationStart>
       <quotationEnd>'</quotationEnd>
       <doubleQuotationStart>&quot;</doubleQuotationStart>
       <doubleQuotationEnd>&quot;</doubleQuotationEnd>
   </delimiters>
   <numbers>
       <numberFormat class="decimal" default="true">
          <!--
           There can be multiple number formats for the same locale.
           Only one can be marked as "default"
          -->
          <!--  The class of symbol together with it's identifier "id=", position and value allows for
            -->
          <patternElement patternid="number" type="decimal" default="false">
              <special owner="StarOffice" msgid="FixedFormatskey5" usage="FIXED_NUMBER" formatindex="4" />
              <patternCode type="positive">#,##0.###</patternCode>
              <patternCode type="negative">-#,##0.###</patternCode>
          </patternElement>
          <patternElement patternid="number" type="percent" default="false">
              <special owner="StarOffice" msgid="FixedFormatskey6" usage="FIXED_NUMBER" formatindex="4" />
              <patternCode type="positive">#,##0%</patternCode>
          </patternElement>
          <patternElement patternid="number" type="scientific" default="false">
              <special owner="StarOffice" msgid="FixedFormatskey7" usage="FIXED_NUMBER" formatindex="4" />
              <patternCode >#,##0%</patternCode>
          </patternElement>

          <symbols>
              <decimal>.</decimal>
              <group>,</group>
              <list>;</list>
              <percentSign>%</percentSign>
              <nativeZeroDigit>0</nativeZeroDigit>
              <patternDigit>#</patternDigit>
              <plusSign>+</plusSign>
              <minusSign>-</minusSign>
              <exponential>E</exponential>
              <perMille>&#x2030;</perMille>
              <infinity>&#x221e;</infinity>
              <nan>&#x2639;</nan>
         </symbols>
       </numberFormat>
       <currencies>
            <currency id="USD" name="dollar" default="true">
                <symbol>$</symbol>
                <decimal>.</decimal>
                <group>,</group>
                <patternElement patternid="currency" default="false">
                    <special owner="StarOffice" msgid="FixedFormatskey9" usage="FIXED_NUMBER" formatindex="4" />
                    <patternCode type="positive">&#x00a4; #,##0.00;</patternCode>
                    <patternCode type="negative">(&#x00a4; #,##0.00)</patternCode>
                </patternElement>
            </currency>

            <currency id ="JPY" name="yen">
                <name>&#x00A5;</name>
                <patternElement patternid="currency" default="false">
                    <special owner="StarOffice" msgid="FixedFormatskey9" usage="FIXED_NUMBER" formatindex="4" />
                    <patternCode type="positive">&#x00a4; #,##0.00;</patternCode>
                    <patternCode type="negative">(&#x00a4; #,##0.00)</patternCode>
                </patternElement>
                <decimal>.</decimal>
            </currency>
        </currencies>
    </numbers>

    <collation version="1.5" accentOrder="reverse" override="true">

        <!--
        This syntax is an XMLized version of the Java/ICU syntax, used to tailor the default table.
        The main difference is that the &lt; sign is replaced by &#xAB;, and the &amp; is replaced by "&#xA7;",
        since otherwise the XML is clumsy!

        Note that collation needs its own version, independent of the Locale version.
        For more information, see
        http://java.sun.com/products/jdk/1.2/docs/api/java/text/RuleBasedCollator.html
        and
        http://www.unicode.org/unicode/reports/tr10/#Comparison to Java.
        By default, lowercase is before upper in the default.
        This can be changed with the firstCase="upper" attribute.
        French accent ordering can be specified with the accentOrder="reverse" attribute.
        The displayName attribute supplies a message pattern for getDisplayName.
        -->
        <!-- this example takes the swedish tailoring plus part of a japanese
        tailoring, for illustration -->
        <reset>Z</reset>
        <p>&#x00E6;</p>
        <t>&#x00C6;</t>
        <p>a&#x030A;</p>
        <t>A&#x030A;</t>
        <t>aa</t>
        <t>aA</t>
        <t>Aa</t>
        <t>AA</t>
        <p>a&#x0308;</p>
        <t>A&#x0308;</t>
        <p>o&#x0308;</p>
        <t>O&#x0308;</t>
        <s>u&#x030B;</s>
        <t>U&#x030B;</t>
        <p>o&#x030B;</p>
        <t>O&#x030B;</t>
        <s>&#x00F8;</s>
        <t>&#x00D8;</t>
        <reset>V</reset>
        <!-- following is equivalent to <t>w</t><t>W</t><t>u&#x0308;</t><t>U&#x0308;</t> -->
        <tc>wWu&#x0308;U&#x0308;</tc>
        <position at="last non ignorable"/>
        <!-- following is equivalent to <p>&#x4E9C;</p><p>&#x5516;</p><p>&#x5A03;</p>... -->
        <pc>&#x4E9C;&#x5516;&#x5A03;&#x963F;&#x54C0;&#x611B;&#x6328;&#x59F6;&#x9022;&#x8475;&#x831C;&#x7A50;&#x60AA;&#x63E1;&#x6E25;&#x65ED;&#x8466;&#x82A6;</pc>
        <pc>&#x9BF5;&#x6893;&#x5727;&#x65A1;&#x6271;</pc>
        <special owner="OpenOffice">
            <collator unoid="alphanumeric" default="true"/>
            <collator unoid="phonebook" default="false"/>
        </special>
    </collation>

    <boundaries>
         <!-- Boundary rules.
        Selected samples are given with no attempt to make them work.
        This format is the Java/ICU syntax, at the top level.
        For real data, see http://oss.software.ibm.com/developerworks/opensource/cvs/icu4j
        in BreakIteratorRules.java
        displayName attributes removed for now
        -->
        <grapheme class="RuleBased">
            <!-- in addition to the normal rules, treat CH and RR as graphemes. -->
            [cC][hH];[rR][rR]
        </grapheme>
        <word class="Dictionary" import="thaiDict.dat" >
            <!-- When doing Thai word break, check the normal word break rules first. -->
            digit=[[:Nd:][:No:]];
            $digit [[:Pd:]&#xAD;&#x2027;&apos;.]
        </word>
    </boundaries>
    <countryInfo>
        <!-- CountryPost:
             The operand is a string with the abbreviation of the country,
             used for postal addresses, for example by CEPT-MAILCODE. -->
        <countryPost id="US"/>
        <!-- CountryNumber :
             The operand is an integer with the three-digit number of the
              country, according to ISO 3166. -->
        <countryNumber id="666"/>
        <!-- CountryCar:
             The operand is a string with the abbreviation of the country,
             used for motor vehicles and traffic, according to the Geneve convention
             1949:68 -->
        <countryCar id="US"/>
        <!-- CountryISBNNumber :
             The operand is a string with the abbreviation of the country,
             used for book numbering (ISBN), according to ISO 2108. ISBN numbers are
             allocated according to country. -->
        <countryISBNNumber id="666"/>

    </countryInfo>
    <measurement>
        <paperSize>
            <!-- Papersize. The numbers are always in millimeters.
             The units name is the customary unit for that locale for measuring paper. -->
            <height>279</height>
            <width>216</width>
            <units>mm</units>
        </paperSize>
        <!-- Measurement system: US/ISO 1000/Other -->
        <measurementSystem id="US"/>
    </measurement>
    <special owner="OpenOffice">
        <numberingLevels>
            <!--
            the parameter NumType can have one of the following values:
            0 = CHARS_UPPER_LETTER   EX: A, B, C, ....
            1 = CHARS_LOWER_LETTER   EX: a, b, c, ....
            2 = ROMAN_UPPER          EX: I, II, III, ....
            3 = ROMAN_LOWER          EX: i, ii, iii, ....
            4 = ARABIC               EX: 1, 2, 3, 4, .....
            5 = NUMBER_ONE     numbering is invisible
            6 = CHAR_SPECIAL   use a character from specified font
            7 = PAGE_DESCRIPTOR  numbering is specified in page style
            8 = BITMAP                numbering is displayed in bitmap graphic
            9 = CHARS_UPPER_LETTER_N  EX: A, B, ...., AA, BB
            10 = CHARS_LOWER_LETTER_N EX: a, b, ..., aa, bb, cc
           -->
          <numberingLevel Prefix="" NumType="4" Suffix=")" />
          <numberingLevel Prefix="" NumType="4" Suffix="." />
          <numberingLevel Prefix="(" NumType="4" Suffix=")" />
          <numberingLevel Prefix="" NumType="2" Suffix="." />
          <numberingLevel Prefix="" NumType="0" Suffix=")" />
          <numberingLevel Prefix="" NumType="1" Suffix=")" />
          <numberingLevel Prefix="(" NumType="1" Suffix=")" />
          <numberingLevel Prefix="" NumType="3" Suffix="." />
        </numberingLevels>
        <reservedWords>
            <trueWord>true</trueWord>
            <falseWord>false</falseWord>
            <quarter1Word>1st quarter</quarter1Word>
            <quarter2Word>2nd quarter</quarter2Word>
            <quarter3Word>3rd quarter</quarter3Word>
            <quarter4Word>4th quarter</quarter4Word>
            <aboveWord>above</aboveWord>
            <belowWord>below</belowWord>
        </reservedWords>
        <transliterationModules>IGNORE_CASE</transliterationModules>
        <outLineNumberingLevel></outLineNumberingLevel>
    </special>
    <special owner="POSIX">
        <!-- old abbreviations for pre-GUI days -->
        <messages>
            <no>No</no>
            <yes>Yes</yes>
            <shortYes>Y</shortYes>
            <shortNo>N</shortNo>
        </messages>
    </special>
    <special owner="Redhat">
        <!-- The following is ISO TR 14652 compatibility data. (14652 is a Type 1 TR:
             "when the required support cannot be obtained for the publication of an
             International Standard, despite repeated effort".
             See the ballot comments on http://anubis.dkuug.dk/jtc1/sc22/wg20/docs/n948-J1N6769-14652.pdf
             for details on how bad 14652 is.)
             This section is here because TR 14652 data is used in RedHat LINUX.
         -->
       <iso14652:iso14652 xmlns:iso14652="http://oss.software.ibm.com/icu/iso14652">


            <iso14652:addressFormat>
                  <!--

                        Postal format:
                        POSIX          ICU              Description
                        =====      ===========         =============
                        %a         {co_address}         C/O address.
                        %f         {firm_name}          Firm name.
                        %d         {dept_name}          department name.
                        %b         {bldg_name}          Building name.
                        %s         {street}             street or block (eg. Japanese) name.
                        %h         {house_num}          house number or designation.
                        %N         {graphic_chars}      if any graphical characters have been specified then an
                                                        of line is made.
                        %t         {esc_seq_preced}     if the preceding escape sequence resulted in an empty
                                                        string, then the empty string, else a <space>.
                        %r         {room_num}           room number, door designation.
                        %e         {floor_num}          floor number.
                        %C         {cntry_desig}        country designation.
                        %z         {zip}                zip number, postal code.
                        %T         {city}               town, city.
                        %c         {country}            country.

                        Example:
                        POSIX : postal_fmt "<%><a><%><N><%><f><%><N><%><d><%><N><%><b><%><N>/
                                     <%><s><SP><%><h><SP><%><e><SP><%><r><%><N>/
                                     <%><C><-><%><z><SP><%><T><%><N><%><c><%><N>"

                        ICU   : "{co_address}{graphic_chars}{firm_name}{graphic_chars}{dept_name}{graphic_chars}{bldg_name}{graphic_chars}{street}\s{house_num}\s{8}\s{room_num}{graphic_chars}{cntry_desig}-{zip}\s{city}{graphic_chars}{country}{graphic_chars}"
                -->
                <iso14652:postalPattern>{postal_code}{country}{town}{street}{building}{floor}{room}</iso14652:postalPattern>
            </iso14652:addressFormat>
            <iso14652:nameFormat>
                <!-- Grab-bag name format from 14652

                POSIX          ICU                     Description
                =====      ===========                 =============
                %f         {family_name}               Family names.
                %F         {family_name_upper}         Family names in uppercase.
                %g         {first_name}                First given name.
                %G         {first_initial}             First given initial.
                %l         {first_name_latin}          First given name with latin letters.
                %o         {short_name}                Other shorter name, eg. "Bill".
                %m         {middle_name}               Middle names.
                %M         {middle_initial}            Middle initial.
                %p         {profession}                Profession.
                %s         {salutation}                Salutation, such as "Doctor"
                %S         {salutation_abbr}           Abbreviated salutation, such as "Mr." or "Dr."
                %d         {salutation_fdcc}           Salutation, using the FDCC-sets conventions, with 1 for
                                                       the name_gen, 2 for name_mr, 3 for name_mrs, 4 for
                                                       name_miss, 5 for name_ms.
                %t          {esc_seq_preced}           If the preceding escape sequence resulted in an empty
                                                            string, then the empty string, else a <space>.
                Example:
                POSIX    : name_fmt "<%><p><%><t><%><g><%><t><%><m><%><t><%><f>"
                ICU  : NameFormat "{8}\s{2}\s{6}\s{0}"
                -->
                <namePattern> {profession} {first_name} {middle_name} {family_name}</namePattern>
                <generalSalutation></generalSalutation>
                <shortSalutationMr> Mr.</shortSalutationMr>
                <shortSalutationMiss>Ms.</shortSalutationMiss>
                <shortSalutationMrs>Mrs.</shortSalutationMrs>
                <longSalutationMr> Mister.</longSalutationMr>
                <longSalutationMiss>Miss</longSalutationMiss>
                <longSalutationMrs>Mrs.</longSalutationMrs>
            </iso14652:nameFormat>

            <iso14652:identification>
                <!-- Identification of the person who produced the data file. -->
                <iso14652:title></iso14652:title>
                <iso14652:source></iso14652:source>
                <iso14652:address></iso14652:address>
                <iso14652:contact></iso14652:contact>
                <iso14652:email></iso14652:email>
                <iso14652:telephone></iso14652:telephone>
                <iso14652:fax></iso14652:fax>
                <iso14652:language></iso14652:language>
                <iso14652:territory></iso14652:territory>
                <iso14652:audience></iso14652:audience>
                <iso14652:application></iso14652:application>
                <iso14652:abbreviation></iso14652:abbreviation>
                <iso14652:revision></iso14652:revision>
                <iso14652:date></iso14652:date>
            </iso14652:identification>

            <iso14652:telephoneFormat>
                <!--
                        Telephone format:
                        POSIX   ICU                 Description
                        =====   ==========          ===========
                        %a      {area_code}         area code without prefix (prefix is often <0>).
                        %A      {area_code_prefix}  area code including prefix (prefix is often <0>).
                        %l      {local_num}         local number.
                        %c      {country_code}      country code.
                        %C      {alternative_carr}  alternative carrier service code used for dialling
                                                    abroad
                        %c      {contry}            country.

                        Example:
                        POSIX  :  tel_int_fmt "<+><%><c><SP><%><a><SP><%><l>"
                        ICU    :  "+{country_code}\s{area_code}\s{local_num}"
                -->
                <iso14652:internationalPattern> +{country_code} {area_code}-{local_num}</iso14652:internationalPattern>
                <iso14652:domesticPattern>({area_code}) {local_num}</iso14652:domesticPattern>
                <iso14652:internationalDialCode>001 </iso14652:internationalDialCode>
                <iso14652:internationalPrefix> </iso14652:internationalPrefix>
            </iso14652:telephoneFormat>
        </iso14652:iso14652>

     </special>
</localeData>
