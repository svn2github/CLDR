<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE LocaleElements SYSTEM "./LocaleElements.dtd" >
<localeData>
<!--
Locale Data Format
Draft: 2002-10-24

Format that contains the required locale data for use by Java, ICU and POSIX APIs.
Note: constructing a POSIX locale also requires use of the Unicode Character Database data.
That data being common across locales, it is not duplicated here.
See ftp://ftp.unicode.org/Public/UNIDATA/UnicodeCharacterDatabase.html.

NOTE: The data below is purely illustrative, and doesn't match any particular language.
NOTE: The data for any given locale will only contain items that are different from the parent locale.
For example, most country locales will inherit the bulk of their data from the language locale:
"en" will contain the bulk of the data: en_US will only contain a few items like currency.
-->
    <identity>
        <generation date="2002-10-24" />
        <version number="1.1">Various notes and changes in version 1.1</version>
        <!-- Two letter codes, if available; otherwise 3-letter codes.If both exist, a separate database contains the mapping. -->
        <language type="en"/>
        <country type="US"/>

        <!-- Only one variant allowed for now -->
        <variant type="B"/>
        <correspondsTo type="lcid" value="0409"/>
    </identity>
    <localeDisplayNames>
        <!--
        Display names for scripts, languages, countries, and variants in this locale
        -->
        <scripts>
            <!-- IDs come from the Unicode Tech Report on scripts -->
            <script type="Latin">Romana</script>
            <script type="Cyrillic">Kyrillica</script>
         </scripts>

        <languages>
            <language type="ab">Abkhazian</language>
            <language type="aa">Afar</language>
            <language type="af">Afrikaans</language>
            <language type="sq">Albanian</language>
        </languages>

        <territories>
            <territory type="AF">Afghanistan</territory>
            <territory type="AL">Albania</territory>
            <territory type="DZ">Algeria</territory>
            <territory type="AD">Andorra</territory>
            <territory type="AO">Angola</territory>
            <territory type="US">United States</territory>
        </territories>

        <variants>
            <!-- Variant IDs must be unique across ALL locales. Currently they include
            B, NY, EURO -->
            <variant type="B">Bokm&#x00e5;l</variant>
        </variants>
        <keys>
            <key type="collation">Sortierung</key>
        </keys>
        <types>
            <type type="phonebook" key="collation">Telefonbuch</type>
        </types>
    </localeDisplayNames>
    <layout>
        <orientation lines="top-to-bottom" characters="left-to-right" />    <!-- both attributes are required -->
    </layout>
    <encodings>
        <!-- follows the UnicodeSet syntax -->
        <exemplarCharacters>[a-z&#x00E5;&#x00E6;&#x00F8;]</exemplarCharacters>
        <!--
        List of encodings that are in common use in this locale.
        The order is not significant.
        The format is as in http://www.unicode.org/unicode/reports/tr22/
        with the version suppressed.
        (It is assumed that the largest available version is chosen.)
        The exemplarCharacters can also be used to select appropriate encodings.
        -->
       <mapping registry="iana" type="windows-1252"/>
       <mapping registry="iana" type="iso-8859_1"/>
       <mapping registry="iana" type="euc-jp"/>
    </encodings>
    <delimiters>
       <quotationStart>'</quotationStart>
       <quotationEnd>'</quotationEnd>
       <doubleQuotationStart>&#x201C;</doubleQuotationStart>
       <doubleQuotationEnd>&#x201D;</doubleQuotationEnd>
    </delimiters>
    <measurement>
        <paperSize>
            <!-- Papersize. The numbers are always in millimeters.
             The units name is the customary unit for that locale for measuring paper. -->
            <height>279</height>
            <width>216</width>
            <units>mm</units>
        </paperSize>
        <!-- Measurement system: US/ISO 1000/Other -->
        <measurementSystem type="US"/>
    </measurement>
    <dates>
        <!--
        Patterns used for formatting and parsing dates and times for this calendar
        -->
        <localizedPatternChars>GyMdkHmsSEDFwWahKz</localizedPatternChars>
        <calendars>
          <!--
          There can be multiple calendars in the same locale, e.g. Gregorian + Imperial Japanese.
          The type's must match what is produced by the calendar system.
          -->
          <default type="Gregorian"/>
          <calendar type="Gregorian">
              <monthNames>
                  <month type="1">january</month>
                  <month type="2">february</month>
                  <month type="3">march</month>
                  <month type="4">april</month>
                  <month type="5">may</month>
                  <month type="6">june</month>
                  <month type="7">july</month>
                  <month type="8">august</month>
                  <month type="9">September</month>
                  <month type="10">October</month>
                  <month type="11">November</month>
                  <month type="12">December</month>
              </monthNames>
              <monthAbbr>
                  <month type="1">Jan</month>
                  <month type="2">Feb</month>
                  <month type="3">Mar</month>
                  <month type="4">Apr</month>
                  <month type="5">May</month>
                  <month type="6">Jun</month>
                  <month type="7">Jul</month>
                  <month type="8">Aug</month>
                  <month type="9">Sep</month>
                  <month type="10">Oct</month>
                  <month type="11">Nov</month>
                  <month type="12">Dec</month>
              </monthAbbr>
  
              <dayNames>
                  <day type="sun">Sunday</day>
                  <day type="mon">Monday</day>
                  <day type="tue">Tuesday</day>
                  <day type="wed">Wednesday</day>
                  <day type="thu">Thursday</day>
                  <day type="fri">Friday</day>
                  <day type="sat">Saturday</day>
              </dayNames>
              <dayAbbr>
                  <day type="sun">Sun</day>
                  <day type="mon">Mon</day>
                  <day type="tue">Tue</day>
                  <day type="wed">Wed</day>
                  <day type="thu">Thu</day>
                  <day type="fri">Fri</day>
                  <day type="sat">Sat</day>
              </dayAbbr>
              <week>
                  <minDays count="1"/>
                  <firstDay day="sun"/>
                  <weekendStart day="fri" time="18:00"/>
                  <weekendEnd day="sun" time="18:00"/>
              </week>
              <am>AM</am>
              <pm>PM</pm>
  
               <eras>
                  <!-- Non-gregorians may have many more eras -->
                  <era type="0">BC</era>
                  <era type="1">AD</era>
              </eras>
              <dateFormats>
                <default type="medium"/>
                <dateFormat type="medium">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="http://www.openoffice.org" msgtype="DateFormatskey3" usage="DATE" formatindex="20" />
                      <pattern>MM d, yyyy </pattern>
                      <displayName>DIN 5008 (EN 28601)</displayName>
                 </dateFormat>
                 <dateFormat type="full">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="http://www.openoffice.org" msgtype="DateFormatskey3" usage="DATE" formatindex="20" />
                      <pattern>EEEE, MMMM d, yyyy</pattern>
                      <displayName>DIN 5008 (EN 28601)</displayName>
                 </dateFormat>
              </dateFormats>
              <timeFormats>
                  <default type="medium"/>
                  <timeFormat type="full">
                    <!--  Optional special element for platform/application specific overheads. Paser can
                          choose to ignore if required  -->
                      <special owner="http://www.openoffice.org" msgtype="DateFormatskey3" usage="DATE" formatindex="20" />
                      <pattern>h:mm:ss a z</pattern>
                      <displayName>DIN 5008 (EN 28601)</displayName>
                 </timeFormat>
                 <timeFormat type="medium">
                      <pattern>h:mm:ss a</pattern>
                 </timeFormat>
              </timeFormats>
              <dateTimeFormat>
                  <pattern>{0} {1}</pattern>
              </dateTimeFormat>
          </calendar>
  
          <!-- The Buddhist calendar inherits month names, etc. (everything but eras) from Gregorian -->
          <calendar type="Buddhist">
              <eras>
                  <era type="0">BE</era>
              </eras>
          </calendar>
        </calendars>

        <timeZoneNames>
                <!--
                The zone id's are language-independent.
                Their values and data (default offset from GMT, daylight savings rules)
                are stored elsewhere
                Here we just have the localized names for them.
                -->
                <default type="America/Los_Angeles"/>
                <zone type="America/Los_Angeles">
                    <long>
                        <generic>Pacific Time</generic>
                        <standard>Pacific Standard Time</standard>
                        <daylight>Pacific Daylight Time</daylight>
                    </long>
                    <short>
                        <generic>PT</generic>
                        <standard>PST</standard>
                        <daylight>PDT</daylight>
                    </short>
                    <exemplarCity>Los Angeles</exemplarCity>
                </zone>

                <zone type="America/Denver">
                    <long>
                        <generic>Mountain Time</generic>
                        <standard>Mountain Standard Time</standard>
                        <daylight>Mountain Daylight Time</daylight>
                    </long>
                    <short>
                        <generic>MT</generic>
                        <standard>MST</standard>
                        <daylight>MDT</daylight>
                    </short>
                    <exemplarCity>Denver</exemplarCity>
                </zone>

                <zone type="Europe/London">
                     <long>
                        <generic>British Time</generic>
                        <standard>British Standard Time</standard>
                        <daylight>British Daylight Time</daylight>
                    </long>
                    <exemplarCity>York</exemplarCity>
                </zone>

        </timeZoneNames>
   </dates>
   <numbers>
		<symbols>
			<decimal>.</decimal>
			<group>,</group>
			<list>;</list>
			<percentSign>%</percentSign>
			<nativeZeroDigit>0</nativeZeroDigit>
			<patternDigit>#</patternDigit>
			<plusSign>+</plusSign>
			<minusSign>-</minusSign>
			<exponential>E</exponential>
			<perMille>&#x2030;</perMille>
			<infinity>&#x221e;</infinity>
			<nan>&#x2639;</nan>
		</symbols>
		<numberFormats>
			<default type="decimal"/>
			<numberFormat type="decimal">
				<pattern type="positive">#,##0.###</pattern>
				<pattern type="negative">-#,##0.###</pattern>
			</numberFormat>
			<numberFormat type="percent">
				<pattern type="positive">#,##0%</pattern>
			</numberFormat>
			<numberFormat type="scientific">
				<pattern >#,##0%</pattern>
			</numberFormat>
			<numberFormat type="currency">
				<pattern type="positive">&#x00a4; #,##0.00;</pattern>
				<pattern type="negative">(&#x00a4; #,##0.00)</pattern>
			</numberFormat>
		</numberFormats>
		<currencies>
			<default type="USD"/>
			<!-- If no decimal and group seperators are specified that implies
				 they are inherited from symbols section above -->
			<currency type="USD">
				<displayName>dollar</displayName>
				<symbol>$</symbol>
			</currency>
			<currency id ="JPY">
				<displayName>yen</displayName>
				<symbol>&#x00A5;</symbol>
			</currency>
			<currency id ="PTE">
				<displayName>escudo</displayName>
				<symbol>Esc.</symbol>
				<decimal>$</decimal>
			</currency>
		</currencies>
    </numbers>
    <collations>
        <collation>

            <!--
            This syntax is an XMLized version of the Java/ICU syntax, used to tailor the default table.
           
            Note that collation needs its own version, independent of the Locale version.
            For more information, see
            http://java.sun.com/products/jdk/1.2/docs/api/java/text/RuleBasedCollator.html
            and
            http://www.unicode.org/unicode/reports/tr10/#Comparison to Java.
            By default, lowercase is before upper in the default.
            This can be changed with the firstCase="upper" attribute.
            French accent ordering can be specified with the accentOrder="reverse" attribute.
            The displayName attribute supplies a message pattern for getDisplayName.
            -->
			<base UCA="3.1.1"/>
			<settings caseLevel="on"/>
			<rules>
				<!-- this example takes the swedish tailoring plus part of a japanese tailoring, 
				for illustration -->
				<reset>Z</reset>
				<p>&#x00E6;</p>
				<t>&#x00C6;</t>
				<p>a&#x030A;</p>
				<t>A&#x030A;</t>
				<t>aa</t>
				<t>aA</t>
				<t>Aa</t>
				<t>AA</t>
				<p>a&#x0308;</p>
				<t>A&#x0308;</t>
				<p>o&#x0308;</p>
				<t>O&#x0308;</t>
				<s>u&#x030B;</s>
				<t>U&#x030B;</t>
				<p>o&#x030B;</p>
				<t>O&#x030B;</t>
				<s>&#x00F8;</s>
				<t>&#x00D8;</t>
				<reset>V</reset>
				<tc>wW</tc>
				<reset>Y</reset>
				<tc>&#x00FC;&#x00DC;</tc>
				<position at="last non ignorable"/>
				<!-- following is equivalent to <p>&#x4E9C;</p><p>&#x5516;</p><p>&#x5A03;</p>... -->
				<pc>&#x4E9C;&#x5516;&#x5A03;&#x963F;&#x54C0;&#x611B;&#x6328;&#x59F6;&#x9022;&#x8475;&#x831C;&#x7A50;&#x60AA;&#x63E1;&#x6E25;&#x65ED;&#x8466;&#x82A6;</pc>
				<pc>&#x9BF5;&#x6893;&#x5727;&#x65A1;&#x6271;</pc>
			</rules>
            <special owner="http://www.openoffice.org">
                <default type="alphanumeric"/>
                <collator type="alphanumeric" />
                <collator type="phonebook"/>
            </special>
        </collation>
        <collation key="phonebook">
            <alias source="de_DE" type="phonebook"/>
        </collation>
    </collations>
    <special owner="http://oss.software.ibm.com/icu/">
        <ruleBasedNumberFormats>
            <ruleBasedNumberFormat type="spellout">
                %%and:
                    and =%default=;
                    100: =%default=;
                %%commas:
                    ' and =%default=;
                    100: , =%default=;
                    1000: , 
            </ruleBasedNumberFormat>
            <ruleBasedNumberFormat type="ordinal">
                %main:
                    =#,##0==%%abbrev=;
                %%abbrev:
                    th; st; nd; rd; th;
                    20: &gt;&gt;;
                    100: &gt;&gt;;
            </ruleBasedNumberFormat>
            <ruleBasedNumberFormat type="duration">
            %with-words:
                0 seconds; 1 second; =0= seconds;
                60/60: 
            </ruleBasedNumberFormat>
        </ruleBasedNumberFormats>
        <boundaries>
             <!-- Boundary rules.
            Selected samples are given with no attempt to make them work.
            This format is the Java/ICU syntax, at the top level.
            For real data, see http://oss.software.ibm.com/developerworks/opensource/cvs/icu4j
            in BreakIteratorRules.java
            displayName attributes removed for now
            -->
            <grapheme class="RuleBased">
                <!-- in addition to the normal rules, treat CH and RR as graphemes. -->
                [cC][hH];[rR][rR]
            </grapheme>
            <word class="Dictionary" import="thaiDict.dat" >
                <!-- When doing Thai word break, check the normal word break rules first. -->
                digit=[[:Nd:][:No:]];
                $digit [[:Pd:]&#xAD;&#x2027;&apos;.]
            </word>
        </boundaries>
        <transforms>
            <transform target="Latin">
                ::NFD (NFC) ; # convert everything to decomposed for simplicity
                ...
                &#x03B1; &lt;> a ;  &#x0391; &lt;> A ;
                &#x03B2; &lt;> v ;  &#x0392; &lt;> V ;
                &#x03B3; } $gammaLike &lt;> n } $egammaLike ;
                &#x0393; } $gammaLike &lt;> N } $egammaLike ;
                &#x03B3; &lt;> g ;  &#x0393; &lt;> G ;
                &#x03B4; &lt;> d ;  &#x0394; &lt;> D ;
                &#x03B5; &lt;> e ;  &#x0395; &lt;> E ;
                &#x03B6; &lt;> z ;  &#x0396; &lt;> Z ;
                &#x0398; } $beforeLower &lt;> Th ;
                &#x03B8; &lt;> th ;  &#x0398; &lt;> TH ;
                &#x03B9; &lt;> i ;  &#x0399; &lt;> I ;
                &#x03BA; &lt;> k ;  &#x039A; &lt;> K ;
                &#x03BB; &lt;> l ;  &#x039B; &lt;> L ;
                &#x03BC; &lt;> m ; &#x039C; &lt;> M ;
                &#x03BD; } $gammaLike > n\' ;
                &#x039D; } $gammaLike &lt;> N\' ;
                &#x03BD; &lt;> n ; &#x039D; &lt;> N ;
                ...
                ::NFC (NFD) ; # convert back to composed
            </transform>
        </transforms>
    </special>
    <special owner="http://www.openoffice.org">
        <numberingLevels>
            <!--
            the parameter NumType can have one of the following values:
            0 = CHARS_UPPER_LETTER   EX: A, B, C, ....
            1 = CHARS_LOWER_LETTER   EX: a, b, c, ....
            2 = ROMAN_UPPER          EX: I, II, III, ....
            3 = ROMAN_LOWER          EX: i, ii, iii, ....
            4 = ARABIC               EX: 1, 2, 3, 4, .....
            5 = NUMBER_ONE     numbering is invisible
            6 = CHAR_SPECIAL   use a character from specified font
            7 = PAGE_DESCRIPTOR  numbering is specified in page style
            8 = BITMAP                numbering is displayed in bitmap graphic
            9 = CHARS_UPPER_LETTER_N  EX: A, B, ...., AA, BB
            10 = CHARS_LOWER_LETTER_N EX: a, b, ..., aa, bb, cc
           -->
          <numberingLevel Prefix="" NumType="4" Suffix=")" />
          <numberingLevel Prefix="" NumType="4" Suffix="." />
          <numberingLevel Prefix="(" NumType="4" Suffix=")" />
          <numberingLevel Prefix="" NumType="2" Suffix="." />
          <numberingLevel Prefix="" NumType="0" Suffix=")" />
          <numberingLevel Prefix="" NumType="1" Suffix=")" />
          <numberingLevel Prefix="(" NumType="1" Suffix=")" />
          <numberingLevel Prefix="" NumType="3" Suffix="." />
        </numberingLevels>
        <reservedWords>
            <trueWord>true</trueWord>
            <falseWord>false</falseWord>
            <quarter1Word>1st quarter</quarter1Word>
            <quarter2Word>2nd quarter</quarter2Word>
            <quarter3Word>3rd quarter</quarter3Word>
            <quarter4Word>4th quarter</quarter4Word>
            <aboveWord>above</aboveWord>
            <belowWord>below</belowWord>
        </reservedWords>
        <transliterationModules>IGNORE_CASE</transliterationModules>
        <outLineNumberingLevel></outLineNumberingLevel>
    </special>
    <special owner="http://www.opengroup.org/regproducts/xu.htm">
        <!-- old abbreviations for pre-GUI days -->
        <messages>
            <no>No</no>
            <yes>Yes</yes>
            <shortYes>Y</shortYes>
            <shortNo>N</shortNo>
        </messages>
    </special>
    <special owner="http://www.gnu.org/directory/glibc.html">
        <!-- The following is ISO TR 14652 compatibility data.For details on TR 14652 see http://anubis.dkuug.dk/jtc1/sc22/wg20/docs/n972-14652ft.pdf.
         -->
       <iso14652:iso14652 xmlns:iso14652="http://oss.software.ibm.com/locale/locale.html">
            <iso14652:addressFormat>
                <iso14652:postalPattern>"%n%N%a%N%d%N%f%N%b%N%h %s%N%e %r%N%l%N%C-%z %T%, %S %z%N%c%N"</iso14652:postalPattern>
            </iso14652:addressFormat>

            <iso14652:nameFormat>
                <namePattern>%p%t%g%m%t%f</namePattern>
                <generalSalutation></generalSalutation>
                <shortSalutationMr> Mr.</shortSalutationMr>
                <shortSalutationMiss>Ms.</shortSalutationMiss>
                <shortSalutationMrs>Mrs.</shortSalutationMrs>
                <longSalutationMr> Mister.</longSalutationMr>
                <longSalutationMiss>Miss</longSalutationMiss>
                <longSalutationMrs>Mrs.</longSalutationMrs>
            </iso14652:nameFormat>

            <iso14652:identification>
                <!-- Identification of the person who produced the data file. -->
                <iso14652:title></iso14652:title>
                <iso14652:source></iso14652:source>
                <iso14652:address></iso14652:address>
                <iso14652:contact></iso14652:contact>
                <iso14652:email></iso14652:email>
                <iso14652:telephone></iso14652:telephone>
                <iso14652:fax></iso14652:fax>
                <iso14652:language></iso14652:language>
                <iso14652:territory></iso14652:territory>
                <iso14652:audience></iso14652:audience>
                <iso14652:application></iso14652:application>
                <iso14652:abbreviation></iso14652:abbreviation>
                <iso14652:revision></iso14652:revision>
                <iso14652:date></iso14652:date>
            </iso14652:identification>

            <iso14652:telephoneFormat>
                <iso14652:internationalPattern>+%c (%a)%t-%l</iso14652:internationalPattern>
                <iso14652:domesticPattern>(%a)%t-%l</iso14652:domesticPattern>
                <iso14652:internationalDialCode>001 </iso14652:internationalDialCode>
                <iso14652:internationalPrefix> </iso14652:internationalPrefix>
            </iso14652:telephoneFormat>

            <iso14652:countryInfo>
              <iso14652:countryPost>US</iso14652:countryPost>
              <iso14652:countryCar>US</iso14652:countryCar>
              <iso14652:countryNumber type="666"/>
              <iso14652:countryISBNNumber type="666"/>
            </iso14652:countryInfo>
         </iso14652:iso14652>
     </special>
</localeData>
